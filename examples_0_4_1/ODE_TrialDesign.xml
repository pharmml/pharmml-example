                <ParameterEstimation>
                    <ct:SymbRef blkIdRef="pm1" symbIdRef="KDE"/>
                    <InitialEstimate>
                        <ct:Real>10</ct:Real>
                    </InitialEstimate>
                </ParameterEstimation>
                <ParameterEstimation>
                    <ct:SymbRef blkIdRef="om1" symbIdRef="a"/>
