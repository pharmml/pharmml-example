 						<ds:Column columnId="GLU" columnType="dv" valueType="real" columnNum="3" /> 
 					</ds:Definition> 
 					<ds:ImportData oid="id1"> 
 						<ds:path>mm_glu.csv</ds:path> 
 						<ds:format>CSV</ds:format> 
 						<ds:delimiter>COMMA</ds:delimiter> 
 					</ds:ImportData> 
