            <!-- random variable can be declared with variability reference only -->
            <RandomVariable symbId="eta_V">
                <ct:VariabilityReference>
                    <ct:SymbRef symbIdRef="indiv"/>
                </ct:VariabilityReference>
            </RandomVariable>
            
            <Correlation deviationMatrixType="CovMatrix">
