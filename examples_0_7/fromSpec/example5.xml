                    </RandomEffects>
                </StructuredModel>
            </IndividualParameter>
            
        </ParameterModel>

        <!-- STRUCTURAL MODEL - ODEs -->
        <StructuralModel blkId="sm1">
            
            <!-- dC/dt -->
            <ct:DerivativeVariable symbolType="real" symbId="C">
                <ct:Assign>
                    <Equation xmlns="http://www.pharmml.org/pharmml/0.6/Maths">
                        <Binop op="times">
                            <Uniop op="minus">
                                <ct:SymbRef blkIdRef="pm1" symbIdRef="KDE"/>
                            </Uniop>
                            <ct:SymbRef symbIdRef="C"/>
                        </Binop>
                    </Equation>
                </ct:Assign>
                <ct:IndependentVariable>
                    <ct:SymbRef symbIdRef="time"/>
                </ct:IndependentVariable>
                <ct:InitialCondition>
                    <ct:InitialValue>
                        <ct:Assign>
                            <ct:SymbRef blkIdRef="pm1" symbIdRef="C0"/>
                        </ct:Assign>
                    </ct:InitialValue>
                </ct:InitialCondition>
            </ct:DerivativeVariable>
            
            <!-- dPT/dt -->
            <ct:DerivativeVariable symbolType="real" symbId="PT">
                <ct:Assign>
                    <Equation xmlns="http://www.pharmml.org/pharmml/0.6/Maths">
                        <Binop op="plus">
                            <Binop op="times">
                                <ct:SymbRef blkIdRef="pm1" symbIdRef="LAMBDAP"/>
                                <Binop op="times">
                                    <ct:SymbRef symbIdRef="PT"/>
                                    <Binop op="minus">
                                        <ct:Real>1</ct:Real>
                                        <Binop op="divide">
                                            <ct:SymbRef symbIdRef="PSTAR"/>
                                            <ct:SymbRef blkIdRef="pm1" symbIdRef="K"/>
                                        </Binop>
                                    </Binop>
                                </Binop>
                            </Binop>
                            <Binop op="minus">
                                <Binop op="times">
                                    <ct:SymbRef blkIdRef="pm1" symbIdRef="KQPP"/>
                                    <ct:SymbRef symbIdRef="QP"/>
                                </Binop>
                                <Binop op="plus">
                                    <Binop op="times">
                                        <ct:SymbRef blkIdRef="pm1" symbIdRef="KPQ"/>
                                        <ct:SymbRef symbIdRef="PT"/>
                                    </Binop>
                                    <Binop op="times">
                                        <ct:SymbRef blkIdRef="pm1" symbIdRef="GAMMA"/>
                                        <Binop op="times">
                                            <ct:SymbRef symbIdRef="C"/>
                                            <Binop op="times">
                                                <ct:SymbRef blkIdRef="pm1" symbIdRef="KDE"/>
                                                <ct:SymbRef symbIdRef="PT"/>
                                            </Binop>
                                        </Binop>
                                    </Binop>
                                </Binop>
                            </Binop>
                        </Binop>
                    </Equation>
                </ct:Assign>
                <ct:IndependentVariable>
                    <ct:SymbRef symbIdRef="time"/>
                </ct:IndependentVariable>
                <ct:InitialCondition>
                    <ct:InitialValue>
                        <ct:Assign>
                            <ct:SymbRef blkIdRef="pm1" symbIdRef="P0"/>
                        </ct:Assign>
                    </ct:InitialValue>
                </ct:InitialCondition>
            </ct:DerivativeVariable>
            
            <!-- dQ/dt --> 
            <ct:DerivativeVariable symbolType="real" symbId="Q">
                <ct:Assign>
                    <Equation xmlns="http://www.pharmml.org/pharmml/0.6/Maths">
                        <Binop op="minus">
                            <Binop op="times">
                                <ct:SymbRef blkIdRef="pm1" symbIdRef="KPQ"/>
                                <ct:SymbRef symbIdRef="PT"/>
                            </Binop>
                            <Binop op="times">
                                <ct:SymbRef  blkIdRef="pm1" symbIdRef="GAMMA"/>
                                <Binop op="times">
                                    <ct:SymbRef symbIdRef="C"/>
                                    <Binop op="times">
                                        <ct:SymbRef blkIdRef="pm1" symbIdRef="KDE"/>
                                        <ct:SymbRef symbIdRef="Q"/>
                                    </Binop>
                                </Binop>
                            </Binop>
                        </Binop>
                    </Equation>
                </ct:Assign>
                <ct:IndependentVariable>
                    <ct:SymbRef symbIdRef="time"/>
                </ct:IndependentVariable>
                <ct:InitialCondition>
                    <ct:InitialValue>
                        <ct:Assign>
                            <ct:SymbRef blkIdRef="pm1" symbIdRef="Q0"/>
                        </ct:Assign>
                    </ct:InitialValue>
                </ct:InitialCondition>
            </ct:DerivativeVariable>
            
            <!-- dQP/dt -->
            <ct:DerivativeVariable symbolType="real" symbId="QP">
                <ct:Assign>
                    <Equation xmlns="http://www.pharmml.org/pharmml/0.6/Maths">
                        <Binop op="minus">
                            <Binop op="minus">
                                <Binop op="times">
                                    <ct:SymbRef blkIdRef="pm1" symbIdRef="GAMMA"/>
                                    <Binop op="times">
                                        <ct:SymbRef symbIdRef="C"/>
                                        <Binop op="times">
                                            <ct:SymbRef blkIdRef="pm1" symbIdRef="KDE"/>
                                            <ct:SymbRef symbIdRef="Q"/>
                                        </Binop>
                                    </Binop>
                                </Binop>
                                <Binop op="times">
                                    <ct:SymbRef blkIdRef="pm1" symbIdRef="KQPP"/>
                                    <ct:SymbRef symbIdRef="QP"/>
                                </Binop>
                            </Binop>
                            <Binop op="times">
                                <ct:SymbRef blkIdRef="pm1" symbIdRef="DELTAQP"/>
                                <ct:SymbRef symbIdRef="QP"/>
                            </Binop>                        
                        </Binop>
                    </Equation>
                </ct:Assign>
                <ct:IndependentVariable>
                    <ct:SymbRef symbIdRef="time"/>
                </ct:IndependentVariable>
                <ct:InitialCondition>
                    <ct:InitialValue>
                        <ct:Assign>
                            <ct:SymbRef blkIdRef="pm1" symbIdRef="QP0"/>
                        </ct:Assign>
                    </ct:InitialValue>
                </ct:InitialCondition>
            </ct:DerivativeVariable>

            <!-- PSTAR -->
            <ct:Variable symbolType="real" symbId="PSTAR" >
                <ct:Assign>
                    <Equation xmlns="http://www.pharmml.org/pharmml/0.6/Maths">
                        <Binop op="plus">
                            <ct:SymbRef symbIdRef="PT"/>
                            <Binop op="plus">
                                <ct:SymbRef symbIdRef="Q"/>
                                <ct:SymbRef symbIdRef="QP"/>
                            </Binop>
                        </Binop>
                    </Equation>
                </ct:Assign>
            </ct:Variable>
        </StructuralModel>

        <!-- OBSERVATION MODEL - PSTAR -->
        <ObservationModel blkId="om1">
            <ContinuousData>
                <PopulationParameter symbId="a"/>
                <RandomVariable symbId="eps">
                    <ct:VariabilityReference>
                        <ct:SymbRef blkIdRef="vm2" symbIdRef="residual"/>
                    </ct:VariabilityReference>
                    <Distribution>
                        <ProbOnto name="Normal1">
                            <Parameter name="mean">
                                <ct:Assign>
                                    <ct:Real>0</ct:Real>
                                </ct:Assign>
                            </Parameter>
                            <Parameter name="stdev">
                                <ct:Assign>
                                    <ct:Real>1</ct:Real>
                                </ct:Assign>
                            </Parameter>
                        </ProbOnto>
                    </Distribution>
                </RandomVariable>
                <Standard symbId="PSTAR_obs">
                    <Output>
                        <ct:SymbRef blkIdRef="sm1" symbIdRef="PSTAR"/>
                    </Output>
                    <ErrorModel>
                        <ct:Assign>
                            <Equation xmlns="http://www.pharmml.org/pharmml/0.6/Maths">
                                <FunctionCall>
                                    <ct:SymbRef symbIdRef="constantErrorModel"/>
                                    <FunctionArgument symbId="a">
                                        <ct:SymbRef symbIdRef="a"/>
                                    </FunctionArgument>
                                </FunctionCall>
                            </Equation>
                        </ct:Assign>
                    </ErrorModel>
                    <ResidualError>
                        <ct:SymbRef symbIdRef="eps"/>
                    </ResidualError>
                </Standard>
            </ContinuousData>
        </ObservationModel>

    </ModelDefinition>
    
