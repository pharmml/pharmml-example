                </ct:VariabilityReference>
                <Distribution>
                    <ProbOnto name="Normal1">
                        <Parameter name="mean">
                            <ct:Assign>
                                <ct:Real>0</ct:Real>
                            </ct:Assign>
                        </Parameter>
                        <Parameter name="stdev">
                            <ct:Assign>
                                <ct:SymbRef symbIdRef="omega_P0"/>
                            </ct:Assign>
                        </Parameter>
                    </ProbOnto>
                </Distribution>
            </RandomVariable>
            <IndividualParameter symbId="P0">
                <StructuredModel>
                    <Transformation type="log"/>
                    <LinearCovariate>
                        <PopulationValue>
                            <ct:Assign>
                                <ct:SymbRef symbIdRef="pop_P0"/>
                            </ct:Assign>
                        </PopulationValue>
                    </LinearCovariate>
                    <RandomEffects>
                        <ct:SymbRef symbIdRef="eta_P0"/>
                    </RandomEffects>
                </StructuredModel>
            </IndividualParameter>            
            <!-- Q0 -->
            <PopulationParameter symbId="pop_Q0"/>
            <PopulationParameter symbId="omega_Q0"/>
            <RandomVariable symbId="eta_Q0">
