                                <ct:SymbRef symbIdRef="omega_GAMMA"/>
                            </ct:Assign>
                        </Parameter>
                    </ProbOnto>
                </Distribution>
            </RandomVariable>
            <IndividualParameter symbId="GAMMA">
                <StructuredModel>
                    <Transformation type="log"/>
                    <LinearCovariate>
                        <PopulationValue>
                            <ct:Assign>
                                <ct:SymbRef symbIdRef="pop_GAMMA"/>
                            </ct:Assign>
                        </PopulationValue>
                    </LinearCovariate>
                    <RandomEffects>
                        <ct:SymbRef symbIdRef="eta_GAMMA"/>
                    </RandomEffects>
                </StructuredModel>
            </IndividualParameter>
            
            <!-- DELTAQP log-normal distributed -->
            <RandomVariable symbId="eta_DELTAQP">
                <ct:VariabilityReference>
                    <ct:SymbRef blkIdRef="vm1" symbIdRef="indiv"/>
                </ct:VariabilityReference>
                <Distribution>
                    <ProbOnto name="Normal1">
                        <Parameter name="mean">
                            <ct:Assign>
                                <ct:Real>0</ct:Real>
                            </ct:Assign>
                        </Parameter>
                        <Parameter name="stdev">
                            <ct:Assign>
                                <ct:SymbRef symbIdRef="omega_DELTAQP"/>
                            </ct:Assign>
                        </Parameter>
                    </ProbOnto>
                </Distribution>
            </RandomVariable>
            <IndividualParameter symbId="DELTAQP">
                <StructuredModel>
                    <Transformation type="log"/>
                    <LinearCovariate>
                        <PopulationValue>
                            <ct:Assign>
                                <ct:SymbRef symbIdRef="pop_DELTAQP"/>
                            </ct:Assign>
