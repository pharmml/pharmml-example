<?xml version="1.0" encoding="UTF-8"?>
<PharmML xmlns="http://www.pharmml.org/pharmml/0.6/PharmML"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.pharmml.org/pharmml/0.6/PharmML http://www.pharmml.org/pharmml/0.6/PharmML"
    xmlns:ct="http://www.pharmml.org/pharmml/0.6/CommonTypes"
    writtenVersion="0.7" id="i1">
    
    <ct:Name>example334</ct:Name>
    
    <ct:Description>Example 3.3.4 from BayesianModels-V1.1.pdf - population scenario, Parameters V, k are correlated</ct:Description>
    
    <IndependentVariable symbId="T"/>
    
    <ModelDefinition xmlns="http://www.pharmml.org/pharmml/0.6/ModelDefinition">
        
        <!-- EXTENDED -->
        <VariabilityModel blkId="model" type="parameterVariability">
            <Level symbId="pop"/>
            <Level symbId="indiv">
                <ParentLevel>
                    <ct:SymbRef symbIdRef="pop"/>
                </ParentLevel>
            </Level>
        </VariabilityModel>
        <VariabilityModel blkId="resErrorModel" type="residualError"> 
            <Level symbId="residual"/>
        </VariabilityModel>

        
        <ParameterModel blkId="pm1">
            
            <!-- prior parameters on "THETA" -->
            <PopulationParameter symbId="MU_POP_K"/>
            
            <PopulationParameter symbId="MU_POP_V"/>
            
            <PopulationParameter symbId="VAR_POP_K"/>
            
            <PopulationParameter symbId="VAR_POP_V"/>
            
            <!-- prior parameters on "OMEGA" -->
            <PopulationParameter symbId="a_OMEGA_T"/>
            
            <PopulationParameter symbId="b_OMEGA_T"/>
            
            <PopulationParameter symbId="R"/>
            
            <PopulationParameter symbId="rho"/>
            
            <PopulationParameter symbId="a_POP_T"/>
            
            <PopulationParameter symbId="b_POP_T"/>
            
            <!-- prior variables and transformations on "THETA" -->
            <PopulationParameter symbId="lMU_POP_K">
                <ct:Assign>
                    <Equation xmlns="http://www.pharmml.org/pharmml/0.6/Maths">
                        <Uniop op="log">
                            <ct:SymbRef symbIdRef="MU_POP_K"/>
                        </Uniop>
                    </Equation>
                </ct:Assign>
            </PopulationParameter>
            
            <PopulationParameter symbId="lMU_POP_V">
                <ct:Assign>
                    <Equation xmlns="http://www.pharmml.org/pharmml/0.6/Maths">
                        <Uniop op="log">
                            <ct:SymbRef symbIdRef="MU_POP_V"/>
                        </Uniop>
                    </Equation>
                </ct:Assign>
            </PopulationParameter>
            
            <PopulationParameter symbId="lPOP_K">
                <ct:VariabilityReference>
                    <ct:SymbRef symbIdRef="pop" blkIdRef="model"/>
                </ct:VariabilityReference>
                <Distribution>              
                    <UncertML>
                        <NormalDistribution xmlns="http://www.uncertml.org/3.0" definition="">
                            <mean>
                                <var varId="lMU_POP_K"/>
                            </mean>
                            <variance>
                                <var varId="VAR_POP_K"/>
                            </variance>
                        </NormalDistribution>
                    </UncertML>
                </Distribution>
            </PopulationParameter>
            
            <PopulationParameter symbId="lPOP_V">
                <ct:VariabilityReference>
                    <ct:SymbRef symbIdRef="pop" blkIdRef="model"/>
                </ct:VariabilityReference>
                <Distribution>                  
                    <UncertML>
                        <NormalDistribution xmlns="http://www.uncertml.org/3.0" definition="">
                            <mean>
                                <var varId="lMU_POP_V"/>
                            </mean>
                            <variance>
                                <var varId="VAR_POP_V"/>
                            </variance>
                        </NormalDistribution>
                    </UncertML>
                </Distribution>
            </PopulationParameter>
            
            <PopulationParameter symbId="POP_K">
                <ct:Assign>
                    <Equation xmlns="http://www.pharmml.org/pharmml/0.6/Maths">
                        <Uniop op="exp">
                            <ct:SymbRef symbIdRef="lPOP_K"/>
                        </Uniop>
                    </Equation>
                </ct:Assign>
            </PopulationParameter>
            
            <PopulationParameter symbId="POP_V">
                <ct:Assign>
                    <Equation xmlns="http://www.pharmml.org/pharmml/0.6/Maths">
                        <Uniop op="exp">
                            <ct:SymbRef symbIdRef="lPOP_V"/>
                        </Uniop>
                    </Equation>
                </ct:Assign>
            </PopulationParameter>
            
            <!-- prior variables and transformations on "OMEGA" -->
            <PopulationParameter symbId="invOMEGA_P">
                <ct:VariabilityReference>
                    <ct:SymbRef symbIdRef="pop" blkIdRef="model"/>
                </ct:VariabilityReference>
                <Distribution>
                    <ProbOnto name="Wishart1">
                        <Parameter name="scaleMatrix">
                            <ct:Assign>
                                <ct:SymbRef symbIdRef="R"/>
                            </ct:Assign>
                        </Parameter>
                        <Parameter name="degreesOfFreedom">
                            <ct:Assign>
                                <ct:SymbRef symbIdRef="rho"/>
                            </ct:Assign>
                        </Parameter>
                    </ProbOnto>
                </Distribution>
            </PopulationParameter>
            
            <PopulationParameter symbId="OMEGA_P">
                <ct:Assign>
                    <Equation xmlns="http://www.pharmml.org/pharmml/0.6/Maths">
                        <MatrixUniop op="inverse">
                            <ct:SymbRef symbIdRef="invOMEGA_P"/>
                        </MatrixUniop>
                    </Equation>
                </ct:Assign>
            </PopulationParameter>
            
            <PopulationParameter symbId="TAU_T">
                <ct:VariabilityReference>
                    <ct:SymbRef symbIdRef="pop" blkIdRef="model"/>
                </ct:VariabilityReference>
                <Distribution>                              
                    <UncertML>
                        <GammaDistribution xmlns="http://www.uncertml.org/3.0" definition="">
                            <shape>
                                <var varId="a_OMEGA_T"/>
                            </shape>
                            <scale>
                                <var varId="b_OMEGA_T"/>
                            </scale>
                        </GammaDistribution>
                    </UncertML>
                </Distribution>
            </PopulationParameter>
            
            <PopulationParameter symbId="OMEGA_T">
                <ct:Assign>
                    <Equation xmlns="http://www.pharmml.org/pharmml/0.6/Maths">
                        <Binop op="divide">
                            <ct:Real>1</ct:Real>
                            <ct:SymbRef symbIdRef="TAU_T"/>
                        </Binop>
                    </Equation>
                </ct:Assign>
            </PopulationParameter>
            
            <!-- prior variables and transformations on "SIGMA" -->
            <PopulationParameter symbId="POP_T">
                <ct:VariabilityReference>
                    <ct:SymbRef symbIdRef="pop" blkIdRef="model"/>
                </ct:VariabilityReference>          
                <Distribution>                      
                    <UncertML>
                        <GammaDistribution xmlns="http://www.uncertml.org/3.0" definition="">
                            <shape>
                                <var varId="a_POP_T"/>
                            </shape>
                            <scale>
                                <var varId="b_POP_T"/>
                            </scale>
                        </GammaDistribution>
                    </UncertML>
                </Distribution>
            </PopulationParameter>
            
            
            <!-- IIV -->
            <PopulationParameter symbId="lPOP_P">
                <ct:Assign>
                    <ct:Vector>
                        <ct:VectorElements>
                            <ct:SymbRef symbIdRef="lPOP_K"/>
                            <ct:SymbRef symbIdRef="lPOP_V"/>
                        </ct:VectorElements>
                    </ct:Vector>
                </ct:Assign>
            </PopulationParameter>
            
            <PopulationParameter symbId="lP">
                <ct:VariabilityReference>
                    <ct:SymbRef symbIdRef="indiv" blkIdRef="model"/>
                </ct:VariabilityReference>
                <Distribution>
                    <ProbOnto name="MultivariateNormal1">
                        <Parameter name="mean">
                            <ct:Assign>
                                <ct:SymbRef symbIdRef="lPOP_P"/>
                            </ct:Assign>
                        </Parameter>
                        <Parameter name="covarianceMatrix">
                            <ct:Assign>
                                <ct:SymbRef symbIdRef="OMEGA_P"/>
                            </ct:Assign>
                        </Parameter>
                    </ProbOnto>
                </Distribution>
            </PopulationParameter>
            
            <RandomVariable symbId="eta_T">
                <ct:VariabilityReference>
                    <ct:SymbRef symbIdRef="indiv" blkIdRef="model"/>
                </ct:VariabilityReference>
                <Distribution>                      
                    <UncertML>
                        <NormalDistribution xmlns="http://www.uncertml.org/3.0" definition="">
                            <mean>
                                <rVal>0</rVal>
                            </mean>
                            <variance>
                                <var varId="OMEGA_T"/>
                            </variance>
                        </NormalDistribution>
                    </UncertML>
                </Distribution>
            </RandomVariable>
            
            <RandomVariable symbId="eps">
                <ct:VariabilityReference>
                    <ct:SymbRef symbIdRef="residual" blkIdRef="resErrorModel"/>
                </ct:VariabilityReference>          
                <Distribution>
                    <UncertML>
                        <NormalDistribution xmlns="http://www.uncertml.org/3.0" definition="">
                            <mean>
                                <rVal>0</rVal>
                            </mean>
                            <variance>
                                <prVal>1</prVal>
                            </variance>
                        </NormalDistribution>
                    </UncertML>
                </Distribution>
            </RandomVariable>

            <IndividualParameter symbId="K">
                <ct:Assign>
                    <Equation xmlns="http://www.pharmml.org/pharmml/0.6/Maths">
                        <Uniop op="exp">
                            <ct:VectorSelector>
                                <ct:SymbRef symbIdRef="lP"/>
                                <ct:Cell>
                                    <ct:Int>1</ct:Int>
                                </ct:Cell>
                            </ct:VectorSelector>
                        </Uniop>
                    </Equation>
                </ct:Assign>
            </IndividualParameter>
            
            <IndividualParameter symbId="V">
                <ct:Assign>
                    <Equation xmlns="http://www.pharmml.org/pharmml/0.6/Maths">
                        <Uniop op="exp">
                            <ct:VectorSelector>
                                <ct:SymbRef symbIdRef="lP"/>
                                <ct:Cell>
                                    <ct:Int>2</ct:Int>
                                </ct:Cell>
                            </ct:VectorSelector>
                        </Uniop>
                    </Equation>
                </ct:Assign>
            </IndividualParameter>
            
            <IndividualParameter symbId="TAU">
                <StructuredModel>                     
                    <Transformation type="log"/>
                    <PopulationValue>
                        <ct:Assign>
                            <ct:SymbRef symbIdRef="POP_T"/>
                        </ct:Assign>
                    </PopulationValue>
                    <RandomEffects>
                        <ct:SymbRef symbIdRef="eta_T"/>
                    </RandomEffects>
                </StructuredModel>
            </IndividualParameter>
            
            <PopulationParameter symbId="SD_ADD">
                <ct:Assign>
                    <Equation xmlns="http://www.pharmml.org/pharmml/0.6/Maths">
                        <Uniop op="sqrt">
                            <Binop op="divide">
                                <ct:Real>1</ct:Real>
                                <ct:SymbRef symbIdRef="TAU"/>
                            </Binop>
                        </Uniop>
                    </Equation>
                </ct:Assign>
            </PopulationParameter>

        </ParameterModel>
        
        <StructuralModel blkId="sm1">
            <ct:Variable symbolType="real" symbId="DOSE"/>
            
            <ct:Variable symbId="C" symbolType="real">
                <ct:Assign>
                    <Equation xmlns="http://www.pharmml.org/pharmml/0.6/Maths">
                        <Binop op="times">
                            <Binop op="divide">
                                <ct:SymbRef symbIdRef="DOSE" blkIdRef="sm1"/>
                                <ct:SymbRef symbIdRef="V" blkIdRef="pm1"/>
                            </Binop>
                            <Uniop op="exp">
                                <Uniop op="minus">
                                    <Binop op="times">
                                        <ct:SymbRef symbIdRef="K" blkIdRef="pm1"/>
                                        <ct:SymbRef symbIdRef="T"/>
                                    </Binop>
                                </Uniop>
                            </Uniop>
                        </Binop>
                    </Equation>
                </ct:Assign>
            </ct:Variable>
            
        </StructuralModel>
        
        <ObservationModel blkId="om1">
            <ContinuousData>                
                <Standard symbId="Y">
                    <Output>
                        <ct:SymbRef symbIdRef="C" blkIdRef="sm1"/>
                    </Output>
                    <ErrorModel>
                        <ct:Assign>
                            <ct:SymbRef symbIdRef="SD_ADD" blkIdRef="pm1"/>
                        </ct:Assign>
                    </ErrorModel>
                    <ResidualError>
                        <ct:SymbRef symbIdRef="eps" blkIdRef="pm1"/>
                    </ResidualError>
                </Standard>
            </ContinuousData>
        </ObservationModel>
    </ModelDefinition>


    <TrialDesign xmlns="http://www.pharmml.org/pharmml/0.6/TrialDesign">
        
        <ExternalDataSet toolName="NONMEM" oid="NMoid">
            <ColumnMapping>
                <ColumnRef xmlns="http://www.pharmml.org/pharmml/0.6/Dataset" columnIdRef="TIME"/>
                <ct:SymbRef symbIdRef="T"/>
            </ColumnMapping>
            <ColumnMapping>
                <ColumnRef xmlns="http://www.pharmml.org/pharmml/0.6/Dataset" columnIdRef="DV"/>
                <ct:SymbRef symbIdRef="Y" blkIdRef="om1"/>
            </ColumnMapping>
            <ColumnMapping>
                <ColumnRef xmlns="http://www.pharmml.org/pharmml/0.6/Dataset" columnIdRef="DOSE"/>
                <ct:SymbRef symbIdRef="DOSE" blkIdRef="sm1"/>
            </ColumnMapping>
            
            <DataSet xmlns="http://www.pharmml.org/pharmml/0.6/Dataset">
                <Definition>
                    <Column columnId="ID" columnType="id" valueType="id" columnNum="1"/>
                    <Column columnId="TIME" columnType="idv" valueType="real" columnNum="2"/>
                    <Column columnId="DV" columnType="dv" valueType="real" columnNum="3"/>
                    <Column columnId="DOSE" columnType="dose" valueType="real" columnNum="4"/>
                </Definition>
                <ExternalFile oid="id1">
                    <path>bo1.csv</path>
                    <format>CSV</format>
                    <delimiter>COMMA</delimiter>
                </ExternalFile>
            </DataSet>
        </ExternalDataSet>
        
    </TrialDesign>
    
    
    <ModellingSteps xmlns="http://www.pharmml.org/pharmml/0.6/ModellingSteps">
        
        <EstimationStep oid="est1">
            
            <ExternalDataSetReference>
                <ct:OidRef oidRef="NMoid"/>
            </ExternalDataSetReference>
            
            <ParametersToEstimate>
                <ParameterEstimation>
                    <ct:SymbRef blkIdRef="pm1" symbIdRef="R"/>
                    <InitialEstimate>
                        <ct:Matrix matrixType="Any">
                            <ct:MatrixRow>
                                <ct:RowIndex><ct:Int>1</ct:Int></ct:RowIndex>
                                <ct:Real>1</ct:Real>
                                <ct:Real>0.1</ct:Real>
                            </ct:MatrixRow>
                            <ct:MatrixRow>
                                <ct:RowIndex><ct:Int>2</ct:Int></ct:RowIndex>
                                <ct:Real>0.1</ct:Real>
                                <ct:Real>1</ct:Real>
                            </ct:MatrixRow>
                        </ct:Matrix>
                    </InitialEstimate>
                </ParameterEstimation>
                <ParameterEstimation>
                    <ct:SymbRef blkIdRef="pm1" symbIdRef="rho"/>
                </ParameterEstimation>
                
                <!-- omitted other parameters -->
            </ParametersToEstimate>
            
            <Operation order="1" opType="estPop"/>
        </EstimationStep>
        
        <StepDependencies>
            <Step>
                <ct:OidRef oidRef="est1"/>
            </Step>
        </StepDependencies>
        
    </ModellingSteps>
    
</PharmML>
