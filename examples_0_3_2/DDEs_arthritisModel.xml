<?xml version="1.0" encoding="UTF-8"?>
<PharmML xmlns="http://www.pharmml.org/2013/03/PharmML"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.pharmml.org/2013/03/PharmML http://www.pharmml.org/2013/03/PharmML"

    xmlns:math="http://www.pharmml.org/2013/03/Maths"
    xmlns:ct="http://www.pharmml.org/2013/03/CommonTypes"
    xmlns:ds="http://www.pharmml.org/2013/08/Dataset"
    xmlns:design="http://www.pharmml.org/2013/03/TrialDesign"
    writtenVersion="0.3.2" id="i1">
    
    <!-- MODEL NAME -->
    <ct:Name>Example 4 - Estimation with IOV</ct:Name>
    
    <ct:Description>Estimation example with IOV1 and with categorical covariates</ct:Description>
    
    <!-- INDEPENDENT VARIABLE -->
    <IndependentVariable symbId="t"/>

    <!-- FUNCTION DEFINITION -->
    <ct:FunctionDefinition symbId="combinedErrorModel" symbolType="real">
        <ct:FunctionArgument symbId="a" symbolType="real"/>
        <ct:FunctionArgument symbId="b" symbolType="real"/>
        <ct:FunctionArgument symbId="f" symbolType="real"/>
        <ct:Definition>
            <Equation xmlns="http://www.pharmml.org/2013/03/Maths" >
                <Binop op="plus">
                    <ct:SymbRef symbIdRef="a"/>
                    <Binop op="times">
                        <ct:SymbRef symbIdRef="b"/>
                        <ct:SymbRef symbIdRef="f"/>
                    </Binop>
                </Binop>
            </Equation>
        </ct:Definition>
    </ct:FunctionDefinition>
    
    <!-- MODEL DEFINITION -->
    <ModelDefinition xmlns="http://www.pharmml.org/2013/03/ModelDefinition">
    
        <!-- VARIABILITY MODEL -->
        <VariabilityModel blkId="modelVar" type="parameterVariability">
            <Level symbId="indiv"/>
            <Level symbId="iov1">
                <ParentLevel>
                    <ct:SymbRef symbIdRef="indiv"/>
                </ParentLevel>
            </Level>
        </VariabilityModel>
        
        <VariabilityModel blkId="obsErr" type="residualError">
            <Level symbId="resErr"/>
        </VariabilityModel>
        
        <!-- COVARIATE MODEL -->
        <CovariateModel blkId="cm1">
            <Covariate symbId="Sex">
                <Categorical>
                    <Category catId="F"/>
                    <Category catId="M"/>
                </Categorical>
            </Covariate>
            <Covariate symbId="Treat">
                <Categorical>
                    <Category catId="A"/>
                    <Category catId="B"/>
                </Categorical>
            </Covariate>
            <Covariate symbId="TreatSeq">
                <Categorical>
                    <Category catId="AB">
                        <ct:Name>A-B</ct:Name>
                    </Category>
                    <Category catId="BA">
                        <ct:Name>B-A</ct:Name>
                    </Category>
                </Categorical>
            </Covariate>
            <Covariate symbId="Occasion">
                <Categorical>
                    <Category catId="occ1">
                        <ct:Name>1</ct:Name>
                    </Category>
                    <Category catId="occ2">
                        <ct:Name>2</ct:Name>
                    </Category>
                </Categorical>
            </Covariate>
        </CovariateModel>

        <!-- PARAMETER MODEL -->
        <ParameterModel blkId="pm1">
            <SimpleParameter symbId="k3"/>
        </ParameterModel>
        
        <!-- STRUCTURAL MODEL -->
        <StructuralModel blkId="sm1">
           
<!--        example from 'MLXTRAN for Monolix', May 2
        t0 = 0
        G_0 = a*exp(b*t)
        ddt_G = k3 - (sigma1*exp(- sigma2*C) + sigma3)*C*G - k1*(1- exp(- k2*t))*G/k2
        ddt_D = - k5*D + k4*delay(G,tau) -->
            
            <ct:DerivativeVariable symbId="G" symbolType="real">
                <ct:Assign>
                    <ct:SymbRef blkIdRef="pm1" symbIdRef="k3"/>
                    <!-- the rest left out -->
                </ct:Assign>
                <ct:IndependentVariable>
                    <ct:SymbRef symbIdRef="t"/>
                </ct:IndependentVariable>
                <ct:History>
                    <ct:HistoryTimeValue>
                        <ct:Assign>
                            <ct:Real>0</ct:Real>
                        </ct:Assign>
                    </ct:HistoryTimeValue>
                    <ct:Assign>
                        <math:Equation>
                            <math:Binop op="times">
                                <ct:SymbRef symbIdRef="a"/>
                                <math:Uniop op="exp">
                                    <math:Binop op="times">
                                        <ct:SymbRef symbIdRef="b"/>
                                        <ct:SymbRef symbIdRef="t"/>
                                    </math:Binop>
                                </math:Uniop>
                            </math:Binop>
                        </math:Equation>
                    </ct:Assign>
                </ct:History>
            </ct:DerivativeVariable>
            
            <!-- ddt_I = k4*G - k4*delay(G,tau) -->
            <!-- I_0 = 2.52-->
            <ct:DerivativeVariable symbId="I" symbolType="real">
                <ct:Assign>
                    <math:Equation>
                        <math:Binop op="minus">
                            <math:Binop op="times">
                                <ct:SymbRef symbIdRef="k4"/>
                                <ct:SymbRef symbIdRef="G"/>
                            </math:Binop>
                            <math:Binop op="times">
                                <ct:SymbRef symbIdRef="k4"/>
                                <ct:Delay>
                                    <ct:SymbRef symbIdRef="G"/>
                                    <ct:SymbRef symbIdRef="tau"/>
                                </ct:Delay>
                            </math:Binop>
                        </math:Binop>
                    </math:Equation>
                </ct:Assign>
                <ct:History>
                    <ct:HistoryTimeValue>
                        <ct:Assign>
                            <ct:Real>0</ct:Real>
                        </ct:Assign>
                    </ct:HistoryTimeValue>
                    <ct:Assign>
                        <ct:Real>2.52</ct:Real>
                    </ct:Assign>
                </ct:History>
            </ct:DerivativeVariable>

            <!-- ddt_D = - k5*D + k4*delay(G,tau) -->
            <!-- D_0 = 0 -->
            <ct:DerivativeVariable symbId="D" symbolType="real">
                <ct:Assign>
                    <math:Equation>
                        <math:Binop op="plus">
                            <math:Binop op="times">
                                <math:Uniop op="minus">
                                    <ct:SymbRef symbIdRef="k5"/>
                                </math:Uniop>
                                <ct:SymbRef symbIdRef="D"/>
                            </math:Binop>
                            <math:Binop op="times">
                                <ct:SymbRef symbIdRef="k4"/>
                                <ct:Delay>
                                    <ct:SymbRef symbIdRef="G"/>
                                    <ct:SymbRef symbIdRef="tau"/>
                                </ct:Delay>
                            </math:Binop>
                        </math:Binop>
                    </math:Equation>
                </ct:Assign>
                <ct:History>
                    <ct:HistoryTimeValue>
                        <ct:Assign>
                            <ct:Real>0</ct:Real>
                        </ct:Assign>
                    </ct:HistoryTimeValue>
                    <ct:Assign>
                        <ct:Real>0</ct:Real>
                    </ct:Assign>
                </ct:History>
            </ct:DerivativeVariable>

        </StructuralModel>
        
        <!-- OBSERVATION MODEL -->
        <ObservationModel blkId="om1">
            <SimpleParameter symbId="a"/>
            <SimpleParameter symbId="b"/>
            <RandomVariable symbId="epsilon_Cc">
                <ct:VariabilityReference>
                    <ct:SymbRef blkIdRef="obsErr" symbIdRef="resErr"/>
                </ct:VariabilityReference>
                <NormalDistribution xmlns="http://www.uncertml.org/3.0" definition="http://www.uncertml.org/distributions/normal">
                    <mean><rVal>0</rVal></mean>
                    <stddev><prVal>1</prVal></stddev>
                </NormalDistribution>
            </RandomVariable>
            <Standard symbId="Cc_obs"> 
                <Output>
                    <ct:SymbRef blkIdRef="sm1" symbIdRef="Cc"/>
                </Output>
                <ErrorModel>
                    <ct:Assign>
                        <math:Equation >
                            <math:FunctionCall>
                                <ct:SymbRef symbIdRef="combinedErrorModel"/>
                                <math:FunctionArgument symbId="a">
                                    <ct:SymbRef symbIdRef="a"/>
                                </math:FunctionArgument>
                                <math:FunctionArgument symbId="b">
                                    <ct:SymbRef symbIdRef="b"/>
                                </math:FunctionArgument>
                                <math:FunctionArgument symbId="f">
                                    <math:Equation >
                                        <ct:SymbRef symbIdRef="Cc"/>
                                    </math:Equation>
                                </math:FunctionArgument>
                            </math:FunctionCall>
                        </math:Equation>
                    </ct:Assign>
                </ErrorModel>
                <ResidualError>
                    <ct:SymbRef symbIdRef="epsilon_Cc"/>
                </ResidualError>
            </Standard>
        </ObservationModel>
    </ModelDefinition>
    
    <!-- TRIAL DESIGN -->
    <TrialDesign xmlns="http://www.pharmml.org/2013/03/TrialDesign">
        
        <!-- STRUCTURE -->
        <Structure>
            <Epoch oid="ep1">
                <Start>
                    <ct:Real>0</ct:Real>
                </Start>
                <End>
                    <ct:Real>180</ct:Real>
                </End>                
                <Order>1</Order>
            </Epoch>
            <Epoch oid="ep2">
                <Start>
                    <ct:Real>0</ct:Real>
                </Start>
                <End>
                    <ct:Real>180</ct:Real>
                </End>
                <Order>2</Order>
            </Epoch>
            <Epoch oid="ep3">
                <Start>
                    <ct:Real>0</ct:Real>
                </Start>
                <End>
                    <ct:Real>10</ct:Real>
                </End>
                <Order>3</Order>
            </Epoch>
            <Arm oid="a1"/>
            <Arm oid="a2"/>
            <Cell oid="c1">
                <EpochRef oidRef="ep1" />
                <ArmRef oidRef="a1"/>
                <SegmentRef oidRef="ta"/>
            </Cell>
            <Cell oid="c2">
                <EpochRef oidRef="ep1" />
                <ArmRef oidRef="a2"/>
                <SegmentRef oidRef="tb"/>
            </Cell>
            <Cell oid="c3">
                <EpochRef oidRef="ep2" />
                <ArmRef oidRef="a1"/>
                <ArmRef oidRef="a2"/>
                <SegmentRef oidRef="wash"/>
            </Cell>
            <Cell oid="c4">
                <EpochRef oidRef="ep3"/>
                <ArmRef oidRef="a1"/>
                <SegmentRef oidRef="tb"/>
            </Cell>
            <Cell oid="c5">
                <EpochRef oidRef="ep3"/>
                <ArmRef oidRef="a2"/>
                <SegmentRef oidRef="ta"/>
            </Cell>
            <Segment oid="ta">
                <ActivityRef oidRef="d1"/>
            </Segment>
            <Segment oid="tb">
                <ActivityRef oidRef="d2"/>
            </Segment>
            <Segment oid="wash">
                <ActivityRef oidRef="w1"/>
            </Segment>
            <Activity oid="d1">
                <Bolus>
                    <DoseAmount inputTarget="parameter"> 
                        <ct:SymbRef blkIdRef="sm1" symbIdRef="D"/>
                        <ct:Assign>
                            <ct:Real>150</ct:Real>
                        </ct:Assign>
                    </DoseAmount>
                    <DosingTimes>
                        <ct:SymbRef blkIdRef="sm1" symbIdRef="tD"/>
                        <ct:Assign>
                            <ct:Sequence>
                                <ct:Begin><ct:Real>0</ct:Real></ct:Begin>
                                <ct:StepSize><ct:Real>12</ct:Real></ct:StepSize>
                                <ct:End><ct:Real>72</ct:Real></ct:End>
                            </ct:Sequence>
                        </ct:Assign>
                    </DosingTimes>
                </Bolus>
            </Activity>
            <Activity oid="d2">
                <Bolus>
                    <DoseAmount inputTarget="parameter"> 
                        <ct:SymbRef blkIdRef="sm1" symbIdRef="D"/>
                        <ct:Assign>
                            <ct:Real>100</ct:Real>
                        </ct:Assign>
                    </DoseAmount>
                    <DosingTimes>
                        <ct:SymbRef blkIdRef="sm1" symbIdRef="tD"/>
                        <ct:Assign>
                            <ct:Sequence>
                                <ct:Begin><ct:Real>0</ct:Real></ct:Begin>
                                <ct:StepSize><ct:Real>24</ct:Real></ct:StepSize>
                                <ct:End><ct:Real>72</ct:Real></ct:End>
                            </ct:Sequence>
                        </ct:Assign>
                    </DosingTimes>
                </Bolus>
            </Activity>
            <Activity oid="w1">
                <Washout/>
            </Activity>
            <!-- type 1 -->
            <ObservationsEvent oid="occasions"> 
                <ArmRef oidRef="a1"/>
                <ArmRef oidRef="a2"/>
                <ct:VariabilityReference>
                    <ct:SymbRef blkIdRef="modelVar" symbIdRef="iov"/>
                </ct:VariabilityReference>
                <ObservationGroup oid="occ1">
                    <EpochRef oidRef="ep1"/>
                </ObservationGroup>
                <ObservationGroup oid="occ2">
                    <EpochRef oidRef="ep3"/>
                </ObservationGroup>
            </ObservationsEvent>
            <!-- alternative type 2 -->
            <!-- ObservationsEvent oid="occasions"> 
                <ArmRef oidRef="a1"/>
                <ArmRef oidRef="a2"/>
                <ct:VariabilityReference>
                    <ct:SymbRef blkIdRef="modelVar" symbIdRef="iov"/>
                </ct:VariabilityReference>
                <ObservationGroup oid="occ1">
                    <Period>
                        <Start><ct:Real>0</ct:Real></Start>
                        <End><ct:Real>180</ct:Real></End>
                    </Period>
                </ObservationGroup>
                <ObservationGroup oid="occ2">
                    <Period>
                        <Start><ct:Real>0</ct:Real>
                        </Start>
                        <End><ct:Real>180</ct:Real></End>
                    </Period>
                </ObservationGroup>
            </ObservationsEvent-->
        </Structure>
        
        <!-- POPULATION -->
        <Population>

            <ct:VariabilityReference>
                <ct:SymbRef blkIdRef="modelVar" symbIdRef="indiv"/>
            </ct:VariabilityReference>

            <ColumnMapping>
                <ds:ColumnRef columnIdRef="SEX"/>
                <ct:SymbRef blkIdRef="cm1" symbIdRef="Sex"/>
                <ds:CategoryMapping>
                    <ds:Map dataSymbol="0" modelSymbol="M"/>
                    <ds:Map dataSymbol="1" modelSymbol="F"/>
                </ds:CategoryMapping>
            </ColumnMapping>

            <ColumnMapping>
                <ds:ColumnRef columnIdRef="TREAT"/>
                <ct:SymbRef blkIdRef="cm1" symbIdRef="Treat"/>
            </ColumnMapping>            

            <ds:DataSet>
                <ds:Definition>
                    <ds:Column columnId="ID" columnType="id" valueType="id" columnNum="1"/>
                    <ds:Column columnId="ARM" columnType="arm" valueType="id" columnNum="2"/>
                    <ds:Column columnId="SEX" columnType="covariate" valueType="id" columnNum="3"/>
                    <ds:Column columnId="EPOCH" columnType="epoch" valueType="id" columnNum="4"/>
                    <ds:Column columnId="TREAT" columnType="covariate" valueType="id" columnNum="5"/>
                </ds:Definition>
                <ds:Table>
                    <ds:Row><ct:Id>i1</ct:Id><ct:Id>a1</ct:Id><ct:Id>M</ct:Id><ct:Id>ep1</ct:Id><ct:Id>A</ct:Id></ds:Row>
                    <ds:Row><ct:Id>i1</ct:Id><ct:Id>a1</ct:Id><ct:Id>M</ct:Id><ct:Id>ep3</ct:Id><ct:Id>B</ct:Id></ds:Row>
                    <ds:Row><ct:Id>i2</ct:Id><ct:Id>a1</ct:Id><ct:Id>M</ct:Id><ct:Id>ep1</ct:Id><ct:Id>A</ct:Id></ds:Row>
                    <ds:Row><ct:Id>i2</ct:Id><ct:Id>a1</ct:Id><ct:Id>M</ct:Id><ct:Id>ep3</ct:Id><ct:Id>B</ct:Id></ds:Row>
                    <ds:Row><ct:Id>i3</ct:Id><ct:Id>a1</ct:Id><ct:Id>M</ct:Id><ct:Id>ep1</ct:Id><ct:Id>A</ct:Id></ds:Row>
                    <ds:Row><ct:Id>i3</ct:Id><ct:Id>a1</ct:Id><ct:Id>M</ct:Id><ct:Id>ep3</ct:Id><ct:Id>B</ct:Id></ds:Row>
                    <ds:Row><ct:Id>i4</ct:Id><ct:Id>a1</ct:Id><ct:Id>M</ct:Id><ct:Id>ep1</ct:Id><ct:Id>A</ct:Id></ds:Row>
                    <ds:Row><ct:Id>i4</ct:Id><ct:Id>a1</ct:Id><ct:Id>M</ct:Id><ct:Id>ep3</ct:Id><ct:Id>B</ct:Id></ds:Row>
                    <ds:Row><ct:Id>i5</ct:Id><ct:Id>a1</ct:Id><ct:Id>M</ct:Id><ct:Id>ep1</ct:Id><ct:Id>A</ct:Id></ds:Row>
                    <ds:Row><ct:Id>i5</ct:Id><ct:Id>a1</ct:Id><ct:Id>M</ct:Id><ct:Id>ep3</ct:Id><ct:Id>B</ct:Id></ds:Row>
                    <ds:Row><ct:Id>i6</ct:Id><ct:Id>a2</ct:Id><ct:Id>M</ct:Id><ct:Id>ep1</ct:Id><ct:Id>B</ct:Id></ds:Row>
                    <ds:Row><ct:Id>i6</ct:Id><ct:Id>a2</ct:Id><ct:Id>M</ct:Id><ct:Id>ep3</ct:Id><ct:Id>A</ct:Id></ds:Row>
                    <ds:Row><ct:Id>i7</ct:Id><ct:Id>a2</ct:Id><ct:Id>F</ct:Id><ct:Id>ep1</ct:Id><ct:Id>B</ct:Id></ds:Row>
                    <ds:Row><ct:Id>i7</ct:Id><ct:Id>a2</ct:Id><ct:Id>F</ct:Id><ct:Id>ep3</ct:Id><ct:Id>A</ct:Id></ds:Row>
                    <ds:Row><ct:Id>i8</ct:Id><ct:Id>a2</ct:Id><ct:Id>F</ct:Id><ct:Id>ep1</ct:Id><ct:Id>B</ct:Id></ds:Row>
                    <ds:Row><ct:Id>i8</ct:Id><ct:Id>a2</ct:Id><ct:Id>F</ct:Id><ct:Id>ep3</ct:Id><ct:Id>A</ct:Id></ds:Row>
                    <ds:Row><ct:Id>i9</ct:Id><ct:Id>a2</ct:Id><ct:Id>M</ct:Id><ct:Id>ep1</ct:Id><ct:Id>B</ct:Id></ds:Row>
                    <ds:Row><ct:Id>i9</ct:Id><ct:Id>a2</ct:Id><ct:Id>M</ct:Id><ct:Id>ep3</ct:Id><ct:Id>A</ct:Id></ds:Row>
                    <ds:Row><ct:Id>i10</ct:Id><ct:Id>a2</ct:Id><ct:Id>M</ct:Id><ct:Id>ep1</ct:Id><ct:Id>B</ct:Id></ds:Row>
                    <ds:Row><ct:Id>i10</ct:Id><ct:Id>a2</ct:Id><ct:Id>M</ct:Id><ct:Id>ep3</ct:Id><ct:Id>A</ct:Id></ds:Row>
                </ds:Table>
            </ds:DataSet>
        </Population>
    </TrialDesign>
    
    <!-- MODELLING STEPS -->
    <ModellingSteps xmlns="http://www.pharmml.org/2013/03/ModellingSteps">
        
        <!-- ESTIMATION -->
        <EstimationStep oid="e1">
            
            <!-- OBJECTIVE DATA -->
            <ObjectiveDataSet>
                
                <ColumnMapping>
                    <ds:ColumnRef columnIdRef="Y"/>
                    <ct:SymbRef blkIdRef="om1" symbIdRef="Cc_obs"/>
                </ColumnMapping>                
                <ColumnMapping>               
                    <ds:ColumnRef columnIdRef="TIME"/>
                    <ct:SymbRef symbIdRef="t"/>
                </ColumnMapping>
                
                <ds:DataSet>
                    <ds:Definition>
                        <ds:Column columnNum="1" columnId="ID" columnType="id" valueType="id"/>
                        <ds:Column columnNum="2" columnId="TIME" columnType="time" valueType="real"/>
                        <ds:Column columnNum="3" columnId="Y" columnType="dv" valueType="real"/>
                    </ds:Definition>                    
                    <ds:Table>
                        <ds:Row>
                            <ct:Id>i1</ct:Id><ct:Real>0</ct:Real><ct:Real>0</ct:Real>
                        </ds:Row>
                    </ds:Table>
                </ds:DataSet>
            </ObjectiveDataSet>

            <!-- PARAMETERS TO ESTIMATE -->
            <ParametersToEstimate id="e1">
                <ParameterEstimation>
                    <ct:SymbRef blkIdRef="pm1" symbIdRef="pop_ka"/>
                    <InitialEstimate fixed="true">
                        <ct:Real>1.5</ct:Real>
                    </InitialEstimate>
                </ParameterEstimation>
                <ParameterEstimation>
                    <ct:SymbRef blkIdRef="pm1" symbIdRef="omega_ka"/>
                    <InitialEstimate fixed="false">
                        <ct:Real>0</ct:Real>
                    </InitialEstimate>
                </ParameterEstimation>
                <ParameterEstimation>
                    <ct:SymbRef blkIdRef="pm1" symbIdRef="pop_V"/>
                    <InitialEstimate>
                        <ct:Real>0.5</ct:Real>
                    </InitialEstimate>
                </ParameterEstimation>
                <ParameterEstimation>
                    <ct:SymbRef blkIdRef="pm1" symbIdRef="omega_V"/>
                    <InitialEstimate>
                        <ct:Real>0</ct:Real>
                    </InitialEstimate>
                </ParameterEstimation>
                <ParameterEstimation>
                    <ct:SymbRef blkIdRef="pm1" symbIdRef="pop_CL"/>
                    <InitialEstimate>
                        <ct:Real>0.5</ct:Real>
                    </InitialEstimate>
                </ParameterEstimation>
                <ParameterEstimation>
                    <ct:SymbRef blkIdRef="pm1" symbIdRef="omega_CL"/>
                    <InitialEstimate>
                        <ct:Real>0</ct:Real>
                    </InitialEstimate>
                </ParameterEstimation>
                <ParameterEstimation>
                    <ct:SymbRef blkIdRef="pm1" symbIdRef="beta_CL"/>
                    <InitialEstimate>
                        <ct:Real>0</ct:Real>
                    </InitialEstimate>
                </ParameterEstimation>
                <ParameterEstimation>
                    <ct:SymbRef blkIdRef="pm1" symbIdRef="beta_CL_occ1"/>
                    <InitialEstimate>
                        <ct:Real>1</ct:Real>
                    </InitialEstimate>
                </ParameterEstimation>
                <ParameterEstimation>
                    <ct:SymbRef blkIdRef="pm1" symbIdRef="gamma_V"/>
                    <InitialEstimate>
                        <ct:Real>1</ct:Real>
                    </InitialEstimate>
                </ParameterEstimation>
                <ParameterEstimation>
                    <ct:SymbRef blkIdRef="pm1" symbIdRef="gamma_CL"/>
                    <InitialEstimate>
                        <ct:Real>1</ct:Real>
                    </InitialEstimate>
                </ParameterEstimation>
                <ParameterEstimation>
                    <ct:SymbRef blkIdRef="pm1" symbIdRef="beta_V"/>
                    <InitialEstimate>
                        <ct:Real>1</ct:Real>
                    </InitialEstimate>
                </ParameterEstimation>
                <ParameterEstimation>
                    <ct:SymbRef blkIdRef="pm1" symbIdRef="beta_V_occ1"/>
                    <InitialEstimate>
                        <ct:Real>1</ct:Real>
                    </InitialEstimate>
                </ParameterEstimation>
                <ParameterEstimation>
                    <ct:SymbRef blkIdRef="pm1" symbIdRef="beta_V_Treat"/>
                    <InitialEstimate>
                        <ct:Real>1</ct:Real>
                    </InitialEstimate>
                </ParameterEstimation>
                <ParameterEstimation>
                    <ct:SymbRef blkIdRef="pm1" symbIdRef="beta_V_TreatSeq"/>
                    <InitialEstimate>
                        <ct:Real>1</ct:Real>
                    </InitialEstimate>
                </ParameterEstimation>
                <ParameterEstimation>
                    <ct:SymbRef blkIdRef="om1" symbIdRef="a"/>
                    <InitialEstimate>
                        <ct:Real>1</ct:Real>
                    </InitialEstimate>
                </ParameterEstimation>
                <ParameterEstimation>
                    <ct:SymbRef blkIdRef="om1" symbIdRef="b"/>
                    <InitialEstimate>
                        <ct:Real>1</ct:Real>
                    </InitialEstimate>
                </ParameterEstimation>
                
            </ParametersToEstimate>

            <!-- OPERATION -->
            <Operation order="1" opType="estPop">
                <ct:Description>Estimate the population parameters in the model.</ct:Description>
                <Property name="someProperty">
                    <ct:Assign><ct:True/></ct:Assign>
                </Property>
                <Algorithm definition="SimulatedAnnealing">
                    <Property name="initalTemp">
                        <ct:Assign><ct:Real>400</ct:Real></ct:Assign>
                    </Property>
                    <Property name="numIterations">
                        <ct:Assign><ct:Int>100</ct:Int></ct:Assign>
                    </Property>
                </Algorithm>
            </Operation>
            <Operation order="2" opType="estIndiv">
                <ct:Description>Estimate conditional values for the individual parameters.</ct:Description>
            </Operation>
        </EstimationStep>
        
        <!-- STEP DEPENDENCIES -->
        <StepDependencies>
            <Step>
                <ct:OidRef oidRef="e1"/>
            </Step>
        </StepDependencies>
    </ModellingSteps>
</PharmML>