                <ct:Assign>
                    <math:Binop op="minus">
                        <ct:SymbRef blkIdRef="sm" symbIdRef="blood_fluxIn"/>
                        <ct:SymbRef blkIdRef="sm" symbIdRef="blood_fluxOut"/>
                    </math:Binop>
                </ct:Assign>
                <ct:InitialCondition>
                    <ct:InitialValue>
                        <ct:Assign>
