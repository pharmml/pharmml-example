            <ct:Variable symbolType="real" symbId="blood_bloodFlow">
                <ct:Assign>
                    <math:Binop op="plus">
                        <ct:SymbRef blkIdRef="pm" symbIdRef="slowlyPerfused_bloodFlow"/>
                        <math:Binop op="plus">
                            <ct:SymbRef blkIdRef="pm" symbIdRef="kidney_bloodFlow"/>
                            <math:Binop op="plus">
