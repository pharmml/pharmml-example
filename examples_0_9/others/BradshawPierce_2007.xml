                            <math:Binop op="divide">
                                <math:Binop op="times">
                                    <ct:SymbRef blkIdRef="pm" symbIdRef="gut_vmax1"/>
                                    <ct:SymbRef blkIdRef="sm" symbIdRef="gut_concentration"/>
