                    <ct:Sum>
                        <ct:Assign>
                            <ct:VectorSelector>
                                <ct:SymbRef symbIdRef="V"/>
                            </ct:VectorSelector>
                        </ct:Assign>
                        <ct:SumIndex>
                            <ct:SymbRef symbIdRef="i"/>
                        </ct:SumIndex>
                        <ct:LowLimit>
                            <ct:Int>1</ct:Int>
                        </ct:LowLimit>
                        <ct:UpLimit>
                            <ct:Assign>
                                <ct:VectorSelector>
                                    <ct:SymbRef symbIdRef="V"/>
                                    <ct:Cell>
                                        <ct:EndIndex/>
                                    </ct:Cell>
                                </ct:VectorSelector>
                            </ct:Assign>
                        </ct:UpLimit>
                    </ct:Sum>
                </ct:Assign>
            </PopulationParameter>
            
            <PopulationParameter symbId="b">
