<?xml version="1.0" encoding="UTF-8"?>
<PharmML xmlns="http://www.pharmml.org/pharmml/0.8/PharmML"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.pharmml.org/pharmml/0.8/PharmML http://www.pharmml.org/pharmml/0.8/PharmML"
    xmlns:ct="http://www.pharmml.org/pharmml/0.8/CommonTypes"
    xmlns:ds="http://www.pharmml.org/pharmml/0.8/Dataset"
    xmlns:math="http://www.pharmml.org/pharmml/0.8/Maths"
    implementedBy="MJS" writtenVersion="0.8.1" id="i1">
    
    
    <!-- MODEL NAME -->
    <ct:Name>task3_onegroup_optimiseFW - design only</ct:Name>
    
    <ct:Description>based on task3_onegroup_optimiseFW.mld - design only with dummy model definition</ct:Description>
    
    <!-- INDEPENDENT VARIABLE -->
    <IndependentVariable symbId="time"/>

    <!-- BLOCK I: MODEL DEFINITION -->
    <ModelDefinition  xmlns="http://www.pharmml.org/pharmml/0.8/ModelDefinition">
        
        <VariabilityModel blkId="vm1" type="parameterVariability">
            <Level symbId="subject"/>
        </VariabilityModel>
        
        <!-- PARAMETER MODEL -->
        <ParameterModel blkId="pm1">
            <PopulationParameter symbId="a">
                <ct:Assign>
                    <math:Binop op="minus">
                        <ct:Real>1</ct:Real>
                        <math:Binop op="plus">
                            <ct:SymbRef symbIdRef="Genp1"/>
                            <ct:SymbRef symbIdRef="Genp2"/>
                        </math:Binop>
                    </math:Binop>
                </ct:Assign>
            </PopulationParameter>
        </ParameterModel>
        
        <StructuralModel blkId="sm1">
            <ct:Variable symbolType="real" symbId="A"/>
        </StructuralModel>
        
        <ObservationModel blkId="om1">
            <ContinuousData>
                <PopulationParameter symbId="a"/>
                <Standard symbId="Y">
                    <Output>
                        <ct:SymbRef blkIdRef="sm1" symbIdRef="A"/>
                    </Output>
                    <ErrorModel>
                        <ct:Assign>
                            <ct:Real>1</ct:Real>
                        </ct:Assign>
                    </ErrorModel>
                    <ResidualError>
                        <ct:SymbRef symbIdRef="blub"/>
                    </ResidualError>
                </Standard>
            </ContinuousData>
        </ObservationModel>

    </ModelDefinition>
    
    <!-- BLOCK II: TRIAL DEFINITION -->
    <TrialDesign xmlns="http://www.pharmml.org/pharmml/0.8/TrialDesign">
        
        <Interventions>
            <Administration oid="admin1">
                <Bolus>
                    <DoseAmount inputTarget="derivativeVariable">
                        <TargetMapping blkIdRef="sm1">
                            <ds:Map modelSymbol="CENTRAL"/>
                        </TargetMapping>
                        <ct:Assign>
                            <ct:Int>100</ct:Int>              
                        </ct:Assign>
                    </DoseAmount>
                    <DosingTimes>
                        <ct:Assign>
                            <ct:Real>0</ct:Real>
                        </ct:Assign>
                    </DosingTimes>
                </Bolus>
            </Administration>
        </Interventions>
        
        <Observations>
            <Observation oid="window1">
                <ObservationTimes>
                    <ct:Assign>
                        <ct:Vector>
                            <ct:VectorElements>
                                <ct:Real>0.5</ct:Real>
                                <ct:Real>2</ct:Real>
                            </ct:VectorElements>
                        </ct:Vector>
                    </ct:Assign>
                </ObservationTimes>
                <Continuous>
                    <ct:SymbRef blkIdRef="om1" symbIdRef="Y"/>
                </Continuous>
            </Observation>
            
            <Observation oid="window2">
                <NumberTimes>
                    <ct:Assign>
                        <ct:Int>0</ct:Int>
                    </ct:Assign>
                </NumberTimes>
                <Continuous>
                    <ct:SymbRef blkIdRef="om1" symbIdRef="Y"/>
                </Continuous>
            </Observation>
            
            <Observation oid="window3">
                <ObservationTimes>
                    <ct:Assign>
                        <ct:Vector>
                            <ct:VectorElements>
                                <ct:Real>16</ct:Real>
                                <ct:Real>30</ct:Real>
                            </ct:VectorElements>
                        </ct:Vector>
                    </ct:Assign>
                </ObservationTimes>
                <Continuous>
                    <ct:SymbRef blkIdRef="om1" symbIdRef="Y"/>
                </Continuous>
            </Observation>
            
            <!-- COMBINATION -->
            <ObservationsCombination oid="samp">
                <Observations>
                    <ObservationRef oidRef="window1"/>
                    <ObservationRef oidRef="window2"/>
                    <ObservationRef oidRef="window3"/>
                </Observations>
            </ObservationsCombination>
        </Observations>
        
        <DesignSpaces>
            <DesignSpace>
                <InterventionRef oidRef="admin1"/>
                <DoseAmount>
                    <ct:Assign>
                        <ct:Vector>
                            <ct:VectorElements>
                                <ct:Real>10</ct:Real>
                                <ct:Real>100</ct:Real>
                                <ct:Real>200</ct:Real>
                            </ct:VectorElements>
                        </ct:Vector>
                    </ct:Assign>
                </DoseAmount>
            </DesignSpace>
            <DesignSpace>
                <ObservationRef oidRef="window1"/>
                <NumberTimes>
                    <ct:Assign>
                        <ct:Vector>
                            <ct:VectorElements>
                                <ct:Real>1</ct:Real>
                                <ct:Real>2</ct:Real>
                            </ct:VectorElements>
                        </ct:Vector>
                    </ct:Assign>
                </NumberTimes>
            </DesignSpace>
            <DesignSpace>
                <ObservationRef oidRef="window1"/>
                <ObservationTimes>
                    <ct:Assign>
                        <ct:Vector>
                            <ct:VectorElements>
                                <ct:Real>0.5</ct:Real>
                                <ct:Real>1</ct:Real>
                                <ct:Real>1.5</ct:Real>
                                <ct:Real>2</ct:Real>
                                <ct:Real>3</ct:Real>
                            </ct:VectorElements>
                        </ct:Vector>
                    </ct:Assign>
                </ObservationTimes>
            </DesignSpace>
            <DesignSpace>
                <ObservationRef oidRef="window2"/>
                <NumberTimes>
                    <ct:Assign>
                        <ct:Vector>
                            <ct:VectorElements>
                                <ct:Real>0</ct:Real>
                                <ct:Real>1</ct:Real>
                            </ct:VectorElements>
                        </ct:Vector>
                    </ct:Assign>
                </NumberTimes>
            </DesignSpace>
            <DesignSpace>
                <ObservationRef oidRef="window2"/>
                <ObservationTimes>
                    <ct:Assign>
                        <ct:Vector>
                            <ct:VectorElements>
                                <ct:Real>4</ct:Real>
                                <ct:Real>6</ct:Real>
                            </ct:VectorElements>
                        </ct:Vector>
                    </ct:Assign>
                </ObservationTimes>
            </DesignSpace>
            <DesignSpace>
                <ObservationRef oidRef="window3"/>
                <NumberTimes>
                    <ct:Assign>
                        <ct:Vector>
                            <ct:VectorElements>
                                <ct:Real>1</ct:Real>
                                <ct:Real>2</ct:Real>
                                <ct:Real>3</ct:Real>
                            </ct:VectorElements>
                        </ct:Vector>
                    </ct:Assign>
                </NumberTimes>
            </DesignSpace>
            <DesignSpace>
                <ObservationRef oidRef="window3"/>
                <ObservationTimes>
                    <ct:Assign>
                        <ct:Vector>
                            <ct:VectorElements>
                                <ct:Real>12</ct:Real>
                                <ct:Real>16</ct:Real>
                                <ct:Real>20</ct:Real>
                                <ct:Real>24</ct:Real>
                                <ct:Real>28</ct:Real>
                                <ct:Real>30</ct:Real>
                            </ct:VectorElements>
                        </ct:Vector>
                    </ct:Assign>
                </ObservationTimes>
            </DesignSpace>
            <DesignSpace>
                <ObservationRef oidRef="samp"/>
                <NumberTimes>
                    <ct:Assign>
                        <ct:Vector>
                            <ct:VectorElements>
                                <ct:Real>3</ct:Real>
                                <ct:Real>4</ct:Real>
                                <ct:Real>5</ct:Real>
                            </ct:VectorElements>
                        </ct:Vector>
                    </ct:Assign>
                </NumberTimes>
            </DesignSpace>
        </DesignSpaces>
        
        <Arms>
            <TotalSize>
                <ct:Assign>
                    <ct:Int>30</ct:Int>
                </ct:Assign>
            </TotalSize>
            <Arm oid="arm1">
                <ArmSize>
                    <ct:Assign>
                        <ct:Int>30</ct:Int>
                    </ct:Assign>
                </ArmSize>
                <InterventionSequence>
                    <InterventionList>
                        <InterventionRef oidRef="admin1"/>
                    </InterventionList>
                </InterventionSequence>
                <ObservationSequence>
                    <ObservationList>
                        <ObservationRef oidRef="samp"/>
                    </ObservationList>
                    <Start>
                        <ct:Assign>
                            <ct:Real>0</ct:Real>
                        </ct:Assign>
                    </Start>
                </ObservationSequence>
            </Arm>
        </Arms>
        
    </TrialDesign>
    
    
    <ModellingSteps xmlns="http://www.pharmml.org/pharmml/0.8/ModellingSteps">
        
        <TargetTool oid="targetTool">
            <TargetToolName>PFIM</TargetToolName>
        </TargetTool>
        
        <OptimalDesignStep oid="evaTask1">
            
            <TargetToolReference>
                <ct:OidRef oidRef="targetTool"/>
            </TargetToolReference>
            
            <OptimiseOn>
                <DoseAmount>
                    <ct:Assign>
                        <ct:Real>50</ct:Real>
                    </ct:Assign>
                </DoseAmount>
                <ObservationTimes/>
            </OptimiseOn>
            
            <FIM type="B"/>
            <Method>
                <ApproximateFIM type="full"/>
                <OptimizationAlgorithm type="FedorovWynn"/>
            </Method>
            <Cost>
                <TotalCost>
                    <ct:Assign>
                        <ct:Real>1000</ct:Real>
                    </ct:Assign>
                </TotalCost>
            </Cost>
            
            <Operation opType="evaluation" order="1"/>
        </OptimalDesignStep>
        
        <!-- STEP DEPENDENCIES -->
        <StepDependencies>
            <Step>
                <ct:OidRef oidRef="evaTask1"/>
            </Step>
        </StepDependencies>
    </ModellingSteps>
</PharmML>
