<?xml version="1.0" encoding="UTF-8"?>
<PharmML xmlns="http://www.pharmml.org/pharmml/0.8/PharmML"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.pharmml.org/pharmml/0.8/PharmML http://www.pharmml.org/pharmml/0.8/PharmML"
    xmlns:ct="http://www.pharmml.org/pharmml/0.8/CommonTypes"
    xmlns:ds="http://www.pharmml.org/pharmml/0.8/Dataset"
    xmlns:math="http://www.pharmml.org/pharmml/0.8/Maths"
    xmlns:mdef="http://www.pharmml.org/pharmml/0.8/ModelDefinition"
    xmlns:po="http://www.pharmml.org/probonto/ProbOnto"
    implementedBy="MJS" writtenVersion="0.8.1" id="i1">
    
    
    <!-- MODEL NAME -->
    <ct:Name>task4_fourgroup_optimisePopED - design only</ct:Name>
    
    <ct:Description>based on task4_fourgroup_optimisePopED.mld - design only with dummy model definition</ct:Description>
    
    <!-- INDEPENDENT VARIABLE -->
    <IndependentVariable symbId="time"/>

    <!-- BLOCK I: MODEL DEFINITION -->
    <ModelDefinition xmlns="http://www.pharmml.org/pharmml/0.8/ModelDefinition">
        
        <VariabilityModel blkId="vm1" type="parameterVariability">
            <Level symbId="subject"/>
        </VariabilityModel>
        
        <!-- PARAMETER MODEL -->
        <ParameterModel blkId="pm1">
            <DesignParameter symbId="a">
                <ct:Assign>
                        <math:Binop op="minus">
                            <ct:Real>1</ct:Real>
                            <math:Binop op="plus">
                                <ct:SymbRef symbIdRef="Genp1"/>
                                <ct:SymbRef symbIdRef="Genp2"/>
                            </math:Binop>
                        </math:Binop>
                </ct:Assign>
            </DesignParameter>
            
            
            <!-- log(V) âˆ¼ N( log(Vpop), omega_V) -->
            <IndividualParameter symbId="V">
                <Distribution>
                    <po:ProbOnto name="Normal1">
                        <po:Parameter name="mean">
                            <ct:Assign>
                                <math:Uniop op="log">
                                    <ct:SymbRef symbIdRef="Vpop"/>
                                </math:Uniop>
                            </ct:Assign>
                        </po:Parameter>
                        <po:Parameter name="stdev">
                            <ct:Assign>
                                <ct:SymbRef symbIdRef="omega_V"/>
                            </ct:Assign>
                        </po:Parameter>
                    </po:ProbOnto>
                </Distribution>
            </IndividualParameter>
            
        </ParameterModel>
        
                
        <StructuralModel blkId="sm1">
            <ct:Variable symbolType="real" symbId="A"/>
        </StructuralModel>
        
        <ObservationModel blkId="om1">
            <ContinuousData>
                <DesignParameter symbId="a"/>
                <Standard symbId="Y">
                    <Output>
                        <ct:SymbRef blkIdRef="sm1" symbIdRef="A"/>
                    </Output>
                    <ErrorModel>
                        <ct:Assign>
                            <ct:Real>1</ct:Real>
                        </ct:Assign>
                    </ErrorModel>
                    <ResidualError>
                        <ct:SymbRef symbIdRef="blub"/>
                    </ResidualError>
                </Standard>
            </ContinuousData>
        </ObservationModel>
    </ModelDefinition>
    
    <!-- BLOCK II: TRIAL DEFINITION -->
    <TrialDesign xmlns="http://www.pharmml.org/pharmml/0.8/TrialDesign">
        
        <Interventions>
            <Administration oid="d1">
                <Bolus>
                    <DoseAmount inputTarget="derivativeVariable">
                        <TargetMapping blkIdRef="sm1">
                            <ds:Map modelSymbol="CENTRAL"/>
                        </TargetMapping>
                        <ct:Assign>
                            <ct:Int>70</ct:Int>              
                        </ct:Assign>
                    </DoseAmount>
                    <DosingTimes>
                        <ct:Assign>
                            <ct:Real>0</ct:Real>
                        </ct:Assign>
                    </DosingTimes>
                </Bolus>
            </Administration>
            
            <Administration oid="d2">                       <!-- d2 = d1 -->
                <InterventionRef oidRef="d1"/>
            </Administration>
        </Interventions>
        
        <Observations>
            <mdef:DesignParameter symbId="nsamp2">
                <ct:Assign>
                    <ct:Real>5</ct:Real>
                </ct:Assign>
            </mdef:DesignParameter>
            
            <Observation oid="window1">
                <ObservationTimes>
                    <ct:Assign>
                        <ct:Vector>
                            <ct:VectorElements>
                                <ct:Real>0.5</ct:Real>
                                <ct:Real>1</ct:Real>
                                <ct:Real>2</ct:Real>
                            </ct:VectorElements>
                        </ct:Vector>
                    </ct:Assign>
                </ObservationTimes>
                <Continuous>
                    <ct:SymbRef blkIdRef="om1" symbIdRef="Y"/>
                </Continuous>
            </Observation>
            
            <Observation oid="window2">
                <NumberTimes>
                    <ct:Assign>
                        <ct:SymbRef symbIdRef="nsamp2"/>
                    </ct:Assign>
                </NumberTimes>
                <ObservationTimes>
                    <ct:Assign>
                        <ct:Vector>
                            <ct:VectorElements>
                                <ct:Real>6</ct:Real>
                                <ct:Real>24</ct:Real>
                                <ct:Real>36</ct:Real>
                                <ct:Real>72</ct:Real>
                                <ct:Real>120</ct:Real>
                            </ct:VectorElements>
                        </ct:Vector>
                    </ct:Assign>
                </ObservationTimes>
                <Continuous>
                    <ct:SymbRef blkIdRef="om1" symbIdRef="Y"/>
                </Continuous>
            </Observation>
            
            <Observation oid="window3">             <!-- window3 = window1 -->
                <ObservationRef oidRef="window1"/>
            </Observation>
            <Observation oid="window4">
                <ObservationRef oidRef="window2"/>
            </Observation>
            
            <ObservationsCombination oid="samp1">
                <Observations>
                    <ObservationRef oidRef="window1"/>
                    <ObservationRef oidRef="window2"/>
                </Observations>
            </ObservationsCombination>
            <ObservationsCombination oid="samp2">
                <Observations>
                    <ObservationRef oidRef="window3"/>
                    <ObservationRef oidRef="window4"/>
                </Observations>
            </ObservationsCombination>
        </Observations>
        
        <DesignSpaces>
            <DesignSpace>
                <InterventionRef oidRef="d1"/>
                <InterventionRef oidRef="d2"/>
                <DoseAmount>
                    <ct:Assign>
                        <ct:Interval>
                            <ct:LeftEndpoint>
                                <ct:Assign>
                                    <ct:Real>0</ct:Real>
                                </ct:Assign>
                            </ct:LeftEndpoint>
                            <ct:RightEndpoint>
                                <ct:Assign>
                                    <ct:Real>100</ct:Real>
                                </ct:Assign>
                            </ct:RightEndpoint>
                        </ct:Interval>
                    </ct:Assign>
                </DoseAmount>
            </DesignSpace>
            
            <DesignSpace>
                <InterventionRef oidRef="d1"/>
                <InterventionRef oidRef="d2"/>
                <DosingTimes>
                    <ct:Assign>
                        <ct:Interval>
                            <ct:LeftEndpoint>
                                <ct:Assign>
                                    <ct:Real>0</ct:Real>
                                </ct:Assign>
                            </ct:LeftEndpoint>
                            <ct:RightEndpoint>
                                <ct:Assign>
                                    <ct:Real>48</ct:Real>
                                </ct:Assign>
                            </ct:RightEndpoint>
                        </ct:Interval>
                    </ct:Assign>
                </DosingTimes>
            </DesignSpace>
            
            <DesignSpace>
                <ObservationRef oidRef="window1"/>
                <ObservationRef oidRef="window3"/>
                <ObservationTimes>
                    <ct:Assign>
                        <ct:Interval>
                            <ct:LeftEndpoint>
                                <ct:Assign>
                                    <ct:Real>0</ct:Real>
                                </ct:Assign>
                            </ct:LeftEndpoint>
                            <ct:RightEndpoint>
                                <ct:Assign>
                                    <ct:Real>25</ct:Real>
                                </ct:Assign>
                            </ct:RightEndpoint>
                        </ct:Interval>
                    </ct:Assign>
                </ObservationTimes>
            </DesignSpace>
            
            <DesignSpace>
                <ObservationRef oidRef="window1"/>
                <ObservationRef oidRef="window3"/>
                <NumberTimes>
                    <ct:Assign>
                        <ct:Vector>
                            <ct:VectorElements>
                                <ct:Real>1</ct:Real>
                                <ct:Real>2</ct:Real>
                                <ct:Real>3</ct:Real>
                            </ct:VectorElements>
                        </ct:Vector>
                    </ct:Assign>
                </NumberTimes>
            </DesignSpace>
            
            <DesignSpace>
                <ObservationRef oidRef="window2"/>
                <ObservationRef oidRef="window4"/>
                <ObservationTimes>
                    <ct:Assign>
                        <ct:Interval>
                            <ct:LeftEndpoint>
                                <ct:Assign>
                                    <ct:Real>0</ct:Real>
                                </ct:Assign>
                            </ct:LeftEndpoint>
                            <ct:RightEndpoint>
                                <ct:Assign>
                                    <ct:Real>120</ct:Real>
                                </ct:Assign>
                            </ct:RightEndpoint>
                        </ct:Interval>
                    </ct:Assign>
                </ObservationTimes>
            </DesignSpace>
            
            <DesignSpace>
                <ObservationRef oidRef="window2"/>
                <ObservationRef oidRef="window4"/>
                <NumberTimes>
                    <ct:Assign>
                        <ct:Sequence>
                            <ct:Begin>
                                <ct:Real>1</ct:Real>
                            </ct:Begin>
                            <ct:StepSize>
                                <ct:Real>1</ct:Real>
                            </ct:StepSize>
                            <ct:End>
                                <ct:SymbRef symbIdRef="nsamp2"/>        <!-- REFERENCE TO DIFFERENT BLOCK -->
                            </ct:End>
                        </ct:Sequence>
                    </ct:Assign>
                </NumberTimes>
            </DesignSpace>
            
            <DesignSpace>
                <ct:SymbRef symbIdRef="armSize1"/>
                <ct:SymbRef symbIdRef="armSize2"/>
                <ct:SymbRef symbIdRef="armSize3"/>
                <ct:SymbRef symbIdRef="armSize4"/>
                <ct:Assign>
                    <ct:Sequence>
                        <ct:Begin>
                            <ct:Real>0</ct:Real>
                        </ct:Begin>
                        <ct:StepSize>
                            <ct:Real>1</ct:Real>
                        </ct:StepSize>
                        <ct:End>
                            <ct:Real>20</ct:Real>
                        </ct:End>
                    </ct:Sequence>
                </ct:Assign>
            </DesignSpace>
        </DesignSpaces>
        
        <Arms>
            <mdef:DesignParameter symbId="armSize1">
                <ct:Assign>
                    <ct:Real>8</ct:Real>
                </ct:Assign>
            </mdef:DesignParameter>
            <mdef:DesignParameter symbId="armSize2">
                <ct:Assign>
                    <ct:Real>8</ct:Real>
                </ct:Assign>
            </mdef:DesignParameter>
            <mdef:DesignParameter symbId="armSize3">
                <ct:Assign>
                    <ct:Real>8</ct:Real>
                </ct:Assign>
            </mdef:DesignParameter>
            <mdef:DesignParameter symbId="armSize4">
                <ct:Assign>
                    <ct:Real>8</ct:Real>
                </ct:Assign>
            </mdef:DesignParameter>
            
            <Arm oid="arm1">
                <ArmSize>
                    <ct:Assign>
                        <ct:SymbRef symbIdRef="armSize1"/>
                    </ct:Assign>
                </ArmSize>
                <InterventionSequence>
                    <InterventionList>
                        <InterventionRef oidRef="d1"/>
                    </InterventionList>
                </InterventionSequence>
                <ObservationSequence>
                    <ObservationList>
                        <ObservationRef oidRef="samp1"/>
                    </ObservationList>
                    <Start>
                        <ct:Assign>
                            <ct:Real>0</ct:Real>
                        </ct:Assign>
                    </Start>
                </ObservationSequence>
            </Arm>
            
            <Arm oid="arm2">
                <ArmRef oidRef="arm1"/>
                <ArmSize>
                    <ct:Assign>
                        <ct:SymbRef symbIdRef="armSize2"/>
                    </ct:Assign>
                </ArmSize>
                <InterventionSequence>
                    <InterventionList>
                        <InterventionRef oidRef="d2"/>
                    </InterventionList>
                </InterventionSequence>
            </Arm>
            
            <Arm oid="arm3">
                <ArmRef oidRef="arm1"/>
                <ArmSize>
                    <ct:Assign>
                        <ct:SymbRef symbIdRef="armSize3"/>
                    </ct:Assign>
                </ArmSize>
                <ObservationSequence>
                    <ObservationList>
                        <ObservationRef oidRef="samp2"/>
                    </ObservationList>
                </ObservationSequence>
            </Arm>
            <Arm oid="arm4">
                <ArmRef oidRef="arm3"/>
                <ArmSize>
                    <ct:Assign>
                        <ct:SymbRef symbIdRef="armSize4"/>
                    </ct:Assign>
                </ArmSize>
                <InterventionSequence>
                    <InterventionList>
                        <InterventionRef oidRef="d2"/>
                    </InterventionList>
                </InterventionSequence>
            </Arm>
        </Arms>
    </TrialDesign>
    
    
    <ModellingSteps xmlns="http://www.pharmml.org/pharmml/0.8/ModellingSteps">
        
        <TargetTool oid="targetTool">
            <TargetToolName>PopED</TargetToolName>
        </TargetTool>
        
        <OptimalDesignStep oid="evaTask1">
            
            <TargetToolReference>
                <ct:OidRef oidRef="targetTool"/>
            </TargetToolReference>
            
            <OptimiseOn>
                <DoseAmount/>
                <DosingTimes/>
                <NumberTimes/>
                <ObservationTimes/>
                <ct:SymbRef symbIdRef="armSize1"/>
                <ct:SymbRef symbIdRef="armSize2"/>
                <ct:SymbRef symbIdRef="armSize3"/>
                <ct:SymbRef symbIdRef="armSize4"/>
            </OptimiseOn>
            
            <FIM type="P"/>
            <Method>
                <Criterion type="ED"/>
                <ApproximateFIM type="reduced"/>
                <OptimizationAlgorithm type="simplex"/>
            </Method>
            
            <Operation opType="evaluation" order="1"/>
        </OptimalDesignStep>
        
        <!-- STEP DEPENDENCIES -->
        <StepDependencies>
            <Step>
                <ct:OidRef oidRef="evaTask1"/>
            </Step>
        </StepDependencies>
    </ModellingSteps>
</PharmML>
