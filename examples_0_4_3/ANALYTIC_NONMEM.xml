<?xml version="1.0" encoding="UTF-8"?>
<PharmML  xmlns="http://www.pharmml.org/2013/03/PharmML"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.pharmml.org/2013/03/PharmML http://www.pharmml.org/2013/03/PharmML"
    xmlns:math="http://www.pharmml.org/2013/03/Maths"
    xmlns:ct="http://www.pharmml.org/2013/03/CommonTypes"
    xmlns:ds="http://www.pharmml.org/2013/08/Dataset"
    implementedBy="MJS" writtenVersion="0.4.3" id="i1"> 
    
    <!-- MODEL NAME -->
    <ct:Name>Analytic solution based model with NONMEM dataset</ct:Name>
    
    <ct:Description>Analytic Solution based model with NONMEM dataset - for mapping purposes</ct:Description>
    
    <IndependentVariable symbId="t"/>
    
    <!-- residual errors model definition  -->
    <ct:FunctionDefinition symbolType="real" symbId="proportional">
        <ct:FunctionArgument symbolType="real" symbId="b"/>
        <ct:FunctionArgument symbolType="real" symbId="f"/>
        <ct:Definition>
            <math:Equation>
                <math:Binop op="times">
                    <ct:SymbRef symbIdRef="b"/>
                    <ct:SymbRef symbIdRef="f"/>
                </math:Binop>
            </math:Equation>
        </ct:Definition>
    </ct:FunctionDefinition>
    
    <!-- model definition -->
    <ModelDefinition xmlns="http://www.pharmml.org/2013/03/ModelDefinition">
        
        <!-- variability model -->
        <VariabilityModel blkId="randEffect" type="parameterVariability">
            <Level symbId="indiv"/>
        </VariabilityModel>
        
        <VariabilityModel blkId="resErr" type="residualError">
            <Level symbId="residual"/>
        </VariabilityModel>
    
        <!-- covariate model -->
        <CovariateModel blkId="cm1">
            <SimpleParameter symbId="pop_Weight">
                <ct:Assign>
                    <ct:Real>80</ct:Real>
                </ct:Assign>
            </SimpleParameter>
            <SimpleParameter symbId="omega_Weight">
                <ct:Assign>
                    <ct:Real>0.12</ct:Real>
                </ct:Assign>
            </SimpleParameter>
            <Covariate symbId="Weight">
                <Continuous>
                    <Transformation>
                        <TransformedCovariate symbId="tWeight"/>
                        <Equation xmlns="http://www.pharmml.org/2013/03/Maths">
                            <Binop op="power">
                                <Binop op="divide">
                                    <ct:SymbRef symbIdRef="Weight"/>
                                    <ct:Real>70</ct:Real>
                                </Binop>
                                <ct:SymbRef blkIdRef="pm1" symbIdRef="theta2"/>
                            </Binop>
                        </Equation>
                    </Transformation>
                </Continuous>
            </Covariate>
        </CovariateModel>   
        
        <!-- parameter model -->
        <ParameterModel blkId="pm1">
            <SimpleParameter symbId="Ka"/>
            <SimpleParameter symbId="tau"/>
            <SimpleParameter symbId="theta2"/>
            <SimpleParameter symbId="pop_theta1"/>
            <SimpleParameter symbId="omega_theta1"/>
            <SimpleParameter symbId="pop_V"/>
            <SimpleParameter symbId="omega_V"/>            
            <!-- CL -->
            <SimpleParameter symbId="CL">
                <ct:Assign>
                    <Equation xmlns="http://www.pharmml.org/2013/03/Maths">
                        <Binop op="times">
                            <ct:SymbRef symbIdRef="theta1"/>
                            <ct:SymbRef blkIdRef="cm1" symbIdRef="tWeight"/>
                        </Binop>
                    </Equation>
                </ct:Assign>
            </SimpleParameter>
            <!-- theta1 -->
            <RandomVariable symbId="eta_theta1">
                <ct:VariabilityReference>
                    <ct:SymbRef blkIdRef="randEffect" symbIdRef="indiv"/>
                </ct:VariabilityReference>
                <NormalDistribution xmlns="http://www.uncertml.org/3.0" definition="http://www.uncertml.org/distributions/normal">
                    <mean>
                        <rVal>0</rVal>
                    </mean>
                    <stddev>
                        <var varId="omega_theta1"/>
                    </stddev>
                </NormalDistribution>
            </RandomVariable>
            <IndividualParameter symbId="theta1">
                <GaussianModel>
                    <Transformation>log</Transformation>
                    <LinearCovariate>
                        <PopulationParameter>
                            <ct:Assign>
                                <ct:SymbRef symbIdRef="pop_theta1"/>
                            </ct:Assign>
                        </PopulationParameter>
                    </LinearCovariate>
                    <RandomEffects>
                        <ct:SymbRef symbIdRef="eta_theta1"/>
                    </RandomEffects>
                </GaussianModel>
            </IndividualParameter>
            <!-- V -->
            <RandomVariable symbId="eta_V">
                <ct:VariabilityReference>
                    <ct:SymbRef blkIdRef="randEffect" symbIdRef="indiv"/>
                </ct:VariabilityReference>
                <NormalDistribution xmlns="http://www.uncertml.org/3.0" definition="http://www.uncertml.org/distributions/normal">
                    <mean>
                        <rVal>0</rVal>
                    </mean>
                    <stddev>
                        <var varId="omega_V"/>
                    </stddev>
                </NormalDistribution>
            </RandomVariable>
            <IndividualParameter symbId="V">
                <GaussianModel>
                    <Transformation>log</Transformation>
                    <LinearCovariate>
                        <PopulationParameter>
                            <ct:Assign>
                                <ct:SymbRef symbIdRef="pop_V"/>
                            </ct:Assign>
                        </PopulationParameter>
                    </LinearCovariate>
                    <RandomEffects>
                        <ct:SymbRef symbIdRef="eta_V"/>
                    </RandomEffects>
                </GaussianModel>
            </IndividualParameter>
        </ParameterModel>
        
        <!-- structural model -->
        <StructuralModel blkId="sm1">
            <ct:Variable symbolType="real" symbId="D"/>
            <ct:Variable symbolType="real" symbId="tD"/>
            <ct:Variable symbolType="real" symbId="k">
                <ct:Assign>
                    <math:Equation>
                        <math:Binop op="divide">
                            <ct:SymbRef blkIdRef="pm1" symbIdRef="CL"/>
                            <ct:SymbRef blkIdRef="pm1" symbIdRef="V"/>
                        </math:Binop>
                    </math:Equation>
                </ct:Assign>
            </ct:Variable>
            <!-- Css -->
            <ct:Variable symbolType="real" symbId="Css">
                <ct:Assign>
                    <Equation xmlns="http://www.pharmml.org/2013/03/Maths">
                        <Binop op="times">
                            <Binop op="divide">
                                <ct:SymbRef symbIdRef="D"/>
                                <ct:SymbRef blkIdRef="pm1" symbIdRef="V"/>
                            </Binop>
                            <Binop op="times">
                                <Binop op="divide">
                                    <ct:SymbRef blkIdRef="pm1" symbIdRef="Ka"/>
                                    <Binop op="minus">
                                        <ct:SymbRef blkIdRef="pm1" symbIdRef="Ka"/>
                                        <ct:SymbRef symbIdRef="k"/>
                                    </Binop>
                                </Binop>
                                <Binop op="minus">
                                    <Binop op="divide">
                                        <Uniop op="exp">
                                            <Binop op="times">
                                                <Uniop op="minus">
                                                    <ct:SymbRef symbIdRef="k"/>
                                                </Uniop>
                                                <Binop op="minus">
                                                    <ct:SymbRef symbIdRef="t"/>
                                                    <ct:SymbRef symbIdRef="tD"/>                                                    
                                                </Binop>
                                            </Binop>
                                        </Uniop>
                                        <Binop op="minus">
                                            <ct:Real>1</ct:Real>
                                            <Uniop op="exp">
                                                <Binop op="times">
                                                    <Uniop op="minus">
                                                        <ct:SymbRef symbIdRef="k"/>
                                                    </Uniop>
                                                    <ct:SymbRef blkIdRef="pm1" symbIdRef="tau"/>
                                                </Binop>
                                            </Uniop>
                                        </Binop>
                                    </Binop>
                                    <Binop op="divide">
                                        <Uniop op="exp">
                                            <Binop op="times">
                                                <Uniop op="minus">
                                                    <ct:SymbRef blkIdRef="pm1" symbIdRef="Ka"/>
                                                </Uniop>
                                                <Binop op="minus">
                                                    <ct:SymbRef symbIdRef="t"/>
                                                    <ct:SymbRef symbIdRef="tD"/>                                                    
                                                </Binop>
                                            </Binop>
                                        </Uniop>
                                        <Binop op="minus">
                                            <ct:Real>1</ct:Real>
                                            <Uniop op="exp">
                                                <Binop op="times">
                                                    <Uniop op="minus">
                                                        <ct:SymbRef blkIdRef="pm1" symbIdRef="Ka"/>
                                                    </Uniop>
                                                    <ct:SymbRef blkIdRef="pm1" symbIdRef="tau"/>
                                                </Binop>
                                            </Uniop>
                                        </Binop>
                                    </Binop>
                                </Binop>
                            </Binop>
                        </Binop>
                    </Equation>
                </ct:Assign>
            </ct:Variable>
        </StructuralModel>
        
        <!-- observation model -->
        <ObservationModel blkId="om1">
            <ContinuousData>
                <SimpleParameter symbId="b"/>
                <RandomVariable symbId="epsilon_Css">
                    <ct:VariabilityReference>
                        <ct:SymbRef blkIdRef="resErr" symbIdRef="residual"/>
                    </ct:VariabilityReference>
                    <NormalDistribution xmlns="http://www.uncertml.org/3.0" definition="http://www.uncertml.org/distributions/normal">
                        <mean>
                            <rVal>0</rVal>
                        </mean>
                        <stddev>
                            <prVal>1</prVal>
                        </stddev>
                    </NormalDistribution>
                </RandomVariable>
                <Standard symbId="Css_obs">
                    <Output>
                        <ct:SymbRef blkIdRef="sm1" symbIdRef="Css"/>
                    </Output>
                    <ErrorModel>
                        <ct:Assign>
                            <Equation xmlns="http://www.pharmml.org/2013/03/Maths">
                                <FunctionCall>
                                    <ct:SymbRef symbIdRef="proportional"/>
                                    <FunctionArgument symbId="b">
                                        <ct:SymbRef symbIdRef="b"/>
                                    </FunctionArgument>
                                    <FunctionArgument symbId="f">
                                        <ct:SymbRef blkIdRef="sm1" symbIdRef="Css"/>
                                    </FunctionArgument>
                                </FunctionCall>
                            </Equation>
                        </ct:Assign>
                    </ErrorModel>
                    <ResidualError>
                        <ct:SymbRef symbIdRef="epsilon_Css"/>
                    </ResidualError>
                </Standard>
            </ContinuousData>
        </ObservationModel>
    </ModelDefinition>
  
    
    <!-- modeling steps -->
    <ModellingSteps xmlns="http://www.pharmml.org/2013/03/ModellingSteps">
        
        <NONMEMdataSet oid="NMoid">
            
            <ColumnMapping>
                <ds:ColumnRef columnIdRef="TIME"/>
                <ct:SymbRef symbIdRef="t"/>
            </ColumnMapping>
            
            <ColumnMapping>
                <ds:ColumnRef columnIdRef="DV"/>
                <ct:SymbRef blkIdRef="om1" symbIdRef="C_obs"/>
            </ColumnMapping>
            
            <ColumnMapping>
                <ds:ColumnRef columnIdRef="CMT"/>
                <ds:TargetMapping blkIdRef="sm1">
                    <ds:Map dataSymbol="1" modelSymbol="D"/>
                </ds:TargetMapping>
            </ColumnMapping>
            
            
            <DataSet xmlns="http://www.pharmml.org/2013/08/Dataset">
                <Definition>
                    <Column columnId="ID" columnType="id" valueType="id" columnNum="1"/>
                    <Column columnId="TIME" columnType="idv" valueType="real" columnNum="2"/>
                    <Column columnId="DV" columnType="dv" valueType="real" columnNum="3"/>
                    <Column columnId="MDV" columnType="mdv" valueType="int" columnNum="4"/>
                    <Column columnId="DOSE" columnType="dose" valueType="real" columnNum="5"/>
                    <Column columnId="EVID" columnType="evid" valueType="int" columnNum="6"/>
                    <Column columnId="CMT" columnType="cmt" valueType="int" columnNum="7"/>
                </Definition>
                <ImportData oid="importNONMEM">
                    <path>example.csv</path>
                    <format>CSV</format>
                    <delimiter>COMMA</delimiter>
                </ImportData>
            </DataSet>
        </NONMEMdataSet>
        
        <!-- ESTIMATION STEP -->
        <EstimationStep oid="estTask1">
            
            <TargetToolReference>
                <ct:OidRef oidRef="NMoid"/>
            </TargetToolReference>
            
            <ParametersToEstimate>
                
                <!-- a -->
                <ParameterEstimation>
                    <ct:SymbRef blkIdRef="om1" symbIdRef="a"/>
                    <InitialEstimate>
                        <ct:Real>1</ct:Real>
                    </InitialEstimate>
                </ParameterEstimation>
                
                <!-- Ka -->
                <ParameterEstimation>
                    <ct:SymbRef blkIdRef="pm1" symbIdRef="Ka"/>
                    <InitialEstimate>
                        <ct:Real>1</ct:Real>
                    </InitialEstimate>
                </ParameterEstimation>
                
                <!-- omitted other parematers -->
                
            </ParametersToEstimate>
            <Operation order="1" opType="estPop"/>
            <Operation order="2" opType="estIndiv"/>
        </EstimationStep>
        
        <!-- STEP DEPENDENCIES -->
        <StepDependencies>
            <Step>
                <ct:OidRef oidRef="estTask1"/>
            </Step>
        </StepDependencies>
    </ModellingSteps>
    
</PharmML>