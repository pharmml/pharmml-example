<?xml version="1.0" encoding="UTF-8"?>
<PharmML xmlns="http://www.pharmml.org/pharmml/0.6/PharmML"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.pharmml.org/pharmml/0.6/PharmML http://www.pharmml.org/pharmml/0.6/PharmML"
    xmlns:ct="http://www.pharmml.org/pharmml/0.6/CommonTypes"
    writtenVersion="0.6" id="i1">
    
    <ct:Name>example3312</ct:Name>
    
    <ct:Description>Example 3.3.1.2 from BayesianModels-V1.1.pdf - population scenario, all parameters independent, some parameters without prior</ct:Description>
    
    <IndependentVariable symbId="T"/>
    
    <ModelDefinition xmlns="http://www.pharmml.org/pharmml/0.6/ModelDefinition">
        
        <!-- EXTENDED -->
        <VariabilityModel blkId="model" type="parameterVariability">
            <Level symbId="pop"/>
            <Level symbId="indiv">
                <ParentLevel>
                    <ct:SymbRef symbIdRef="pop"/>
                </ParentLevel>
            </Level>
        </VariabilityModel>
        <VariabilityModel blkId="resErrorModel" type="residualError"> 
            <Level symbId="residual"/>
        </VariabilityModel>
        
        <!-- ********** MS: as discussed, this is not necessary and should be replaced by column mapping 
            as implemented in 'ExternalDataSet' columnType="dose" ********** -->
        <!--        <CovariateModel blkId="cm1">
            <Covariate symbId="DOSE">
                <Continuous>
                    <Transformation>
                        <TransformedCovariate symbId="tDOSE"/>
                        <Equation xmlns="http://www.pharmml.org/pharmml/0.6/Maths">
                            <ct:SymbRef symbIdRef="DOSE"/>
                        </Equation>
                    </Transformation>
                </Continuous>
            </Covariate>
        </CovariateModel>-->
        
        <ParameterModel blkId="pm1">
            
            <PopulationParameter symbId="POP_V"/>       <!-- ********** MS: assignments should be done in ModellingsStpes ********** -->
            <PopulationParameter symbId="POP_T"/>
            <PopulationParameter symbId="OMEGA_V"/>
            <PopulationParameter symbId="OMEGA_T"/>
            
            <!-- prior parameters on "THETA" -->
            <PopulationParameter symbId="MU_POP_K"/>

            <PopulationParameter symbId="VAR_POP_K"/>
                        
            <!-- prior parameters on "OMEGA" -->
            <PopulationParameter symbId="a_OMEGA_K"/>
            
            <PopulationParameter symbId="b_OMEGA_K"/>
            
            <!-- prior variables and transformations on "THETA" -->
            <PopulationParameter symbId="lMU_POP_K">
                <ct:Assign>
                    <Equation xmlns="http://www.pharmml.org/pharmml/0.6/Maths">
                        <Uniop op="log">
                            <ct:SymbRef symbIdRef="MU_POP_K"/>
                        </Uniop>
                    </Equation>
                </ct:Assign>
            </PopulationParameter>
            
            <PopulationParameter symbId="lPOP_K">
                <ct:VariabilityReference>
                    <ct:SymbRef symbIdRef="pop" blkIdRef="model"/>
                </ct:VariabilityReference>
                <Distribution>          <!-- ********** MS: missing tags/elements added ********** -->
                    <UncertML>
                        <NormalDistribution xmlns="http://www.uncertml.org/3.0" definition="">
                            <mean>
                                <var varId="lMU_POP_K"/>
                            </mean>
                            <variance>
                                <var varId="VAR_POP_K"/>
                            </variance>
                        </NormalDistribution>
                    </UncertML>
                </Distribution>
            </PopulationParameter>
            
            <PopulationParameter symbId="POP_K">
                <ct:Assign>
                    <Equation xmlns="http://www.pharmml.org/pharmml/0.6/Maths">
                        <Uniop op="exp">
                            <ct:SymbRef symbIdRef="lPOP_K"/>
                        </Uniop>
                    </Equation>
                </ct:Assign>
            </PopulationParameter>
                        
            <!-- prior variables and transformations on "OMEGA" -->
            <PopulationParameter symbId="TAU_K">
                <ct:VariabilityReference>
                    <ct:SymbRef symbIdRef="pop" blkIdRef="model"/>
                </ct:VariabilityReference>
                <Distribution>          <!-- ********** MS: missing tags/elements added ********** -->
                    <UncertML>
                        <GammaDistribution xmlns="http://www.uncertml.org/3.0" definition="">
                            <shape>
                                <var varId="a_OMEGA_K"/>
                            </shape>
                            <scale>
                                <var varId="b_OMEGA_K"/>
                            </scale>
                        </GammaDistribution>
                    </UncertML>
                </Distribution>
            </PopulationParameter>
            
            <PopulationParameter symbId="OMEGA_K">
                <ct:Assign>
                    <Equation xmlns="http://www.pharmml.org/pharmml/0.6/Maths">
                        <Binop op="divide">
                            <ct:Real>1</ct:Real>
                            <ct:SymbRef symbIdRef="TAU_K"/>
                        </Binop>
                    </Equation>
                </ct:Assign>
            </PopulationParameter>
            
            <!-- IIV -->
            <RandomVariable symbId="eta_K">
                <ct:VariabilityReference>
                    <ct:SymbRef symbIdRef="indiv" blkIdRef="model"/>
                </ct:VariabilityReference>
                <Distribution>          <!-- ********** MS: missing tags/elements added ********** -->
                    <UncertML>
                        <NormalDistribution xmlns="http://www.uncertml.org/3.0" definition="">
                            <mean>
                                <rVal>0</rVal>
                            </mean>
                            <variance>
                                <var varId="OMEGA_K"/>
                            </variance>
                        </NormalDistribution>
                    </UncertML>
                </Distribution>
            </RandomVariable>
            
            <RandomVariable symbId="eta_V">
                <ct:VariabilityReference>
                    <ct:SymbRef symbIdRef="indiv" blkIdRef="model"/>
                </ct:VariabilityReference>
                <Distribution>          <!-- ********** MS: missing tags/elements added ********** -->
                    <UncertML>
                        <NormalDistribution xmlns="http://www.uncertml.org/3.0" definition="">
                            <mean>
                                <rVal>0</rVal>
                            </mean>
                            <variance>
                                <var varId="OMEGA_V"/>
                            </variance>
                        </NormalDistribution>
                    </UncertML>
                </Distribution>
            </RandomVariable>
            
            <RandomVariable symbId="eta_T">
                <ct:VariabilityReference>
                    <ct:SymbRef symbIdRef="indiv" blkIdRef="model"/>
                </ct:VariabilityReference>
                <Distribution>          <!-- ********** MS: missing tags/elements added ********** -->
                    <UncertML>
                        <NormalDistribution xmlns="http://www.uncertml.org/3.0" definition="">
                            <mean>
                                <rVal>0</rVal>
                            </mean>
                            <variance>
                                <var varId="OMEGA_T"/>
                            </variance>
                        </NormalDistribution>
                    </UncertML>
                </Distribution>
            </RandomVariable>
            
            <IndividualParameter symbId="K">
                <StructuredModel>                                             <!-- ********** MS: renamed from GaussianModel ********** -->
                    <Transformation>log</Transformation>
                    <LinearCovariate>
                        <PopulationParameter>
                            <ct:Assign>
                                <ct:SymbRef symbIdRef="POP_K"/>
                            </ct:Assign>
                        </PopulationParameter>
                    </LinearCovariate>
                    <RandomEffects>
                        <ct:SymbRef symbIdRef="eta_K"/>
                    </RandomEffects>
                </StructuredModel>
            </IndividualParameter>
            
            <IndividualParameter symbId="V">
                <StructuredModel>                                             <!-- ********** MS: renamed from GaussianModel ********** -->
                    <Transformation>log</Transformation>
                    <LinearCovariate>
                        <PopulationParameter>
                            <ct:Assign>
                                <ct:SymbRef symbIdRef="POP_V"/>
                            </ct:Assign>
                        </PopulationParameter>
                    </LinearCovariate>
                    <RandomEffects>
                        <ct:SymbRef symbIdRef="eta_V"/>
                    </RandomEffects>
                </StructuredModel>
            </IndividualParameter>
            
            <IndividualParameter symbId="TAU">
                <StructuredModel>                                             <!-- ********** MS: renamed from GaussianModel ********** -->
                    <Transformation>log</Transformation>
                    <LinearCovariate>
                        <PopulationParameter>
                            <ct:Assign>
                                <ct:SymbRef symbIdRef="POP_T"/>
                            </ct:Assign>
                        </PopulationParameter>
                    </LinearCovariate>
                    <RandomEffects>
                        <ct:SymbRef symbIdRef="eta_T"/>
                    </RandomEffects>
                </StructuredModel>
            </IndividualParameter>
            
            <!-- SD_ADD can also be defined as SimpleParameter -->
            <PopulationParameter symbId="SD_ADD">                       <!-- ********** MS: renamed from IndividualParameter ********** -->
                <ct:Assign>
                    <Equation xmlns="http://www.pharmml.org/pharmml/0.6/Maths">
                        <Uniop op="sqrt">
                            <Binop op="divide">
                                <ct:Real>1</ct:Real>
                                <ct:SymbRef symbIdRef="TAU"/>
                            </Binop>
                        </Uniop>
                    </Equation>
                </ct:Assign>
            </PopulationParameter>

        </ParameterModel>
        
        <StructuralModel blkId="sm1">
            <ct:Variable symbolType="real" symbId="DOSE"/>
            
            <ct:Variable symbId="C" symbolType="real">
                <ct:Assign>
                    <Equation xmlns="http://www.pharmml.org/pharmml/0.6/Maths">
                        <Binop op="times">
                            <Binop op="divide">
                                <ct:SymbRef symbIdRef="DOSE"/>
                                <ct:SymbRef symbIdRef="V" blkIdRef="pm1"/>
                            </Binop>
                            <Uniop op="exp">
                                <Uniop op="minus">
                                    <Binop op="times">
                                        <ct:SymbRef symbIdRef="K" blkIdRef="pm1"/>
                                        <ct:SymbRef symbIdRef="T"/>
                                    </Binop>
                                </Uniop>
                            </Uniop>
                        </Binop>
                    </Equation>
                </ct:Assign>
            </ct:Variable>
            
        </StructuralModel>
        
        <ObservationModel blkId="om1">
            
            <ContinuousData> 
                <RandomVariable symbId="eps">
                    <ct:VariabilityReference>
                        <ct:SymbRef symbIdRef="residual" blkIdRef="resErrorModel"/>
                    </ct:VariabilityReference>
                    <Distribution>          <!-- ********** MS: missing tags/elements added ********** -->
                        <UncertML>
                            <NormalDistribution xmlns="http://www.uncertml.org/3.0" definition="">
                                <mean>
                                    <rVal>0</rVal>
                                </mean>
                                <variance>
                                    <prVal>1</prVal>
                                </variance>
                            </NormalDistribution>
                        </UncertML>
                    </Distribution>
                </RandomVariable>
                
                <Standard symbId="Y">
                    <Output>
                        <ct:SymbRef symbIdRef="C" blkIdRef="sm1"/>
                    </Output>
                    <ErrorModel>
                        <ct:Assign>
                            <ct:SymbRef symbIdRef="SD_ADD" blkIdRef="pm1"/>
                        </ct:Assign>
                    </ErrorModel>
                    <ResidualError>
                        <ct:SymbRef symbIdRef="eps" blkIdRef="pm1"/>
                    </ResidualError>
                </Standard>
            </ContinuousData>
        </ObservationModel>
    </ModelDefinition>
    
    
    <ModellingSteps xmlns="http://www.pharmml.org/pharmml/0.6/ModellingSteps">
        
        <ExternalDataSet toolName="NONMEM" oid="NMoid">     <!-- ********** MS: renamed from NONMEMdataSet to 'ExternalDataSet toolName="NONMEM"' ********** -->
            <ColumnMapping>
                <ColumnRef xmlns="http://www.pharmml.org/pharmml/0.6/Dataset" columnIdRef="TIME"/>
                <ct:SymbRef symbIdRef="T"/>
            </ColumnMapping>
            <ColumnMapping>
                <ColumnRef xmlns="http://www.pharmml.org/pharmml/0.6/Dataset" columnIdRef="DV"/>
                <ct:SymbRef symbIdRef="Y" blkIdRef="om1"/>
            </ColumnMapping>
            <ColumnMapping>
                <ColumnRef xmlns="http://www.pharmml.org/pharmml/0.6/Dataset" columnIdRef="DOSE"/>
                <ct:SymbRef symbIdRef="DOSE" blkIdRef="sm1"/>               <!-- ********** MS: mapping changed from cm1 to sm1 ********** -->
            </ColumnMapping>
            
            <DataSet xmlns="http://www.pharmml.org/pharmml/0.6/Dataset">
                <Definition>
                    <Column columnId="ID" columnType="id" valueType="id" columnNum="1"/>
                    <Column columnId="TIME" columnType="idv" valueType="real" columnNum="2"/>
                    <Column columnId="DV" columnType="dv" valueType="real" columnNum="3"/>
                    <Column columnId="DOSE" columnType="dose" valueType="real" columnNum="4"/> <!-- ********** MS: new columnType ********** -->
                </Definition>
                <ExternalFile oid="id1">                        <!-- ********** MS: renamed from ImportData ********** -->
                    <path>bo1.csv</path>
                    <format>CSV</format>
                    <delimiter>COMMA</delimiter>
                </ExternalFile>
            </DataSet>
        </ExternalDataSet>
        
        
        <EstimationStep oid="est1">
            
            <ExternalDataSetReference>                            <!-- ********** MS: renamed from NONMEMdataSetReference ********** -->
                <ct:OidRef oidRef="NMoid"/>
            </ExternalDataSetReference>
            
            <ParametersToEstimate>
                <!-- prior parameters on "THETA" -->
                <ParameterEstimation>
                    <ct:SymbRef symbIdRef="POP_V" blkIdRef="pm1"/>
                    <InitialEstimate>
                        <ct:Real>0.1</ct:Real>
                    </InitialEstimate>
                </ParameterEstimation>
                <ParameterEstimation>   
                    <ct:SymbRef symbIdRef="POP_T" blkIdRef="pm1"/>
                    <InitialEstimate>
                        <ct:Real>0.1</ct:Real>
                    </InitialEstimate>
                </ParameterEstimation>
                
                <ParameterEstimation>
                    <ct:SymbRef symbIdRef="OMEGA_V" blkIdRef="pm1"/>
                    <InitialEstimate>
                        <ct:Real>0.1</ct:Real>
                    </InitialEstimate>
                </ParameterEstimation>
                <ParameterEstimation>
                    <ct:SymbRef symbIdRef="OMEGA_T" blkIdRef="pm1"/>
                    <InitialEstimate>
                        <ct:Real>0.1</ct:Real>
                    </InitialEstimate>
                </ParameterEstimation>

                <ParameterEstimation>
                    <ct:SymbRef symbIdRef="MU_POP_K" blkIdRef="pm1"/>
                    <InitialEstimate fixed="true">
                        <ct:Real>0.1</ct:Real>
                    </InitialEstimate>
                </ParameterEstimation>
                <ParameterEstimation>
                    <ct:SymbRef symbIdRef="VAR_POP_K" blkIdRef="pm1"/>
                    <InitialEstimate fixed="true">
                        <ct:Real>0.3</ct:Real>
                    </InitialEstimate>
                </ParameterEstimation>
                
                <!-- prior parameters on "OMEGA" -->
                <ParameterEstimation>
                    <ct:SymbRef symbIdRef="a_OMEGA_K" blkIdRef="pm1"/>
                    <InitialEstimate fixed="true">
                        <ct:Real>0.1</ct:Real>
                    </InitialEstimate>
                </ParameterEstimation>
                <ParameterEstimation>
                    <ct:SymbRef symbIdRef="b_OMEGA_K" blkIdRef="pm1"/>
                    <InitialEstimate fixed="true">
                        <ct:Real>0.1</ct:Real>
                    </InitialEstimate>
                </ParameterEstimation>

                <!-- other parameters skipped -->

            </ParametersToEstimate>
            
            <Operation order="1" opType="estPop"/>
        </EstimationStep>
        
        <StepDependencies>
            <Step>
                <ct:OidRef oidRef="est1"/>
            </Step>
        </StepDependencies>
        
    </ModellingSteps>
    
</PharmML>
