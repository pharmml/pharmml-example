<?xml version="1.0" encoding="UTF-8"?>

<PharmML xmlns="http://www.pharmml.org/2013/03/PharmML"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.pharmml.org/2013/03/PharmML http://www.pharmml.org/2013/03/PharmML"
    xmlns:ct="http://www.pharmml.org/2013/03/CommonTypes"
    writtenVersion="0.3.2" id="i1">
    
    <ct:Name>example3422</ct:Name>
    
    <ct:Description>Example 3.4.2.2 from BayesianModels-V1.1.pdf - population scenario, some parameters correlated (prior level), EMPIRICAL PRIOR DISTRIBUTIONS</ct:Description>
    
    <IndependentVariable symbId="T"/>
    
    <ModelDefinition xmlns="http://www.pharmml.org/2013/03/ModelDefinition">
        
<!-- the "prior" VariabilityModel is not currently implemented -->
        <VariabilityModel blkId="priorModel" type="prior">
            <Level symbId="pri"/>
        </VariabilityModel>
        <VariabilityModel blkId="resErrorModel" type="residualError"> 
            <Level symbId="residual"/>
        </VariabilityModel>
        <VariabilityModel blkId="model" type="parameterVariability">
            <Level symbId="indiv"/>
        </VariabilityModel>
        
        <CovariateModel blkId="cm1">
            <Covariate symbId="DOSE">
                <Continuous>
                    <Transformation>
                        <Equation xmlns="http://www.pharmml.org/2013/03/Maths">
                            <ct:SymbRef symbIdRef="DOSE"/>
                        </Equation>
                    </Transformation>
                </Continuous>
            </Covariate>
        </CovariateModel>
        
        <ParameterModel blkId="pm1">
            
            <!-- prior parameters on "THETA" -->
<!-- the following 2 SimpleParameters are read from INLINE in MDL: -->            
            <SimpleParameter symbId="data_POP_K_V">
                <ct:Assign>
                    <ct:Matrix matrixType="Any">
                        <ct:MatrixRow>
                            <ct:RowIndex><ct:Int>1</ct:Int></ct:RowIndex>
                            <ct:Real>2.006510</ct:Real>
                            <ct:Real>5.050013</ct:Real>
                        </ct:MatrixRow>
                        <ct:MatrixRow>
                            <ct:RowIndex><ct:Int>2</ct:Int></ct:RowIndex>
                            <ct:Real>2.045465</ct:Real>
                            <ct:Real>5.050013</ct:Real> 
                        </ct:MatrixRow>
                        <ct:MatrixRow>
                            <ct:RowIndex><ct:Int>3</ct:Int></ct:RowIndex>
                            <ct:Real>2.084421</ct:Real>
                            <ct:Real>5.050013</ct:Real>
                        </ct:MatrixRow>
                        <ct:MatrixRow>
                            <ct:RowIndex><ct:Int>4</ct:Int></ct:RowIndex>
                            <ct:Real>2.123377</ct:Real>
                            <ct:Real>5.050013</ct:Real>
                        </ct:MatrixRow>
                        <ct:MatrixRow>
                            <ct:RowIndex><ct:Int>5</ct:Int></ct:RowIndex>
                            <ct:Real>2.162333</ct:Real>
                            <ct:Real>5.064166</ct:Real>
                        </ct:MatrixRow>
                        <ct:MatrixRow>
                            <ct:RowIndex><ct:Int>6</ct:Int></ct:RowIndex>
                            <ct:Real>2.201288</ct:Real>
                            <ct:Real>5.064166</ct:Real>
                        </ct:MatrixRow>
                        <ct:MatrixRow>
                            <ct:RowIndex><ct:Int>7</ct:Int></ct:RowIndex>
                            <ct:Real>2.240244</ct:Real>
                            <ct:Real>5.064166</ct:Real> 
                        </ct:MatrixRow>
                        <ct:MatrixRow>
                            <ct:RowIndex><ct:Int>8</ct:Int></ct:RowIndex>
                            <ct:Real>2.279200</ct:Real>
                            <ct:Real>5.064166</ct:Real>
                        </ct:MatrixRow>
                        <ct:MatrixRow>
                            <ct:RowIndex><ct:Int>9</ct:Int></ct:RowIndex>
                            <ct:Real>2.318156</ct:Real>
                            <ct:Real>5.078318</ct:Real>
                        </ct:MatrixRow>
                        <ct:MatrixRow>
                            <ct:RowIndex><ct:Int>10</ct:Int></ct:RowIndex>
                            <ct:Real>2.357111</ct:Real>
                            <ct:Real>5.078318</ct:Real>
                        </ct:MatrixRow>
                    </ct:Matrix>
                </ct:Assign>
            </SimpleParameter>
            
            <!-- prior parameters on "OMEGA" -->
            <SimpleParameter symbId="a_OMEGA_K">
                <ct:Assign>
                    <ct:Real>0.1</ct:Real>
                </ct:Assign>
            </SimpleParameter>
            
            <SimpleParameter symbId="b_OMEGA_K">
                <ct:Assign>
                    <ct:Real>0.1</ct:Real>
                </ct:Assign>
            </SimpleParameter>
            
            <SimpleParameter symbId="a_OMEGA_V">
                <ct:Assign>
                    <ct:Real>0.1</ct:Real>
                </ct:Assign>
            </SimpleParameter>
            
            <SimpleParameter symbId="b_OMEGA_V">
                <ct:Assign>
                    <ct:Real>0.1</ct:Real>
                </ct:Assign>
            </SimpleParameter>
            
            <SimpleParameter symbId="a_OMEGA_T">
                <ct:Assign>
                    <ct:Real>0.3</ct:Real>
                </ct:Assign>
            </SimpleParameter>
            
            <SimpleParameter symbId="b_OMEGA_T">
                <ct:Assign>
                    <ct:Real>0.3</ct:Real>
                </ct:Assign>
            </SimpleParameter>
            
            <SimpleParameter symbId="a_POP_T">
                <ct:Assign>
                    <ct:Real>0.1</ct:Real>
                </ct:Assign>
            </SimpleParameter>
            
            <SimpleParameter symbId="b_POP_T">
                <ct:Assign>
                    <ct:Real>0.1</ct:Real>
                </ct:Assign>
            </SimpleParameter>
            
            <!-- prior variables and transformations on "THETA" -->            
            <RandomVariable symbId="POP_K_V">
                <ct:VariabilityReference>
                    <ct:SymbRef symbIdRef="pri" blkIdRef="priorModel"/>
                </ct:VariabilityReference>
<!-- this distribution name does not exist (it can be changed), it should be like EmpiricalDistribution but in multivariate mode -->
                <MultivariateEmpiricalDistribution xmlns="http://www.uncertml.org/3.0" definition="">
                    <data>
                        <var varId="data_POP_K_V"/>
                    </data>
                </MultivariateEmpiricalDistribution>
            </RandomVariable>
            
<!-- We extract and transform the POP_K_V elements: is it ok to use the random variable POP_K_V as a vector? it should be ok since it has dimension=2 -->
            <IndividualParameter symbId="POP_K">
                <ct:Assign>
                    <Equation xmlns="http://www.pharmml.org/2013/03/Maths">
                        <Uniop op="exp">
                            <ct:VectorSelector>
                                <ct:SymbRef symbIdRef="POP_K_V"/>
                                <ct:Cell>
                                    <ct:Int>1</ct:Int>
                                </ct:Cell>
                            </ct:VectorSelector>
                        </Uniop>
                    </Equation>
                </ct:Assign>
            </IndividualParameter>
            
<!-- We extract and transform the POP_K_V elements: is it ok to use the random variable POP_K_V as a vector? it should be ok since it has dimension=2 -->
            <IndividualParameter symbId="POP_V">
                <ct:Assign>
                    <Equation xmlns="http://www.pharmml.org/2013/03/Maths">
                        <Uniop op="exp">
                            <ct:VectorSelector>
                                <ct:SymbRef symbIdRef="POP_K_V"/>
                                <ct:Cell>
                                    <ct:Int>2</ct:Int>
                                </ct:Cell>
                            </ct:VectorSelector>
                        </Uniop>
                    </Equation>
                </ct:Assign>
            </IndividualParameter>
            
            <!-- prior variables and transformations on "OMEGA" -->
            <RandomVariable symbId="TAU_K">
                <ct:VariabilityReference>
                    <ct:SymbRef symbIdRef="pri" blkIdRef="priorModel"/>
                </ct:VariabilityReference>
                <GammaDistribution xmlns="http://www.uncertml.org/3.0" definition="">
                    <shape>
                        <var varId="a_OMEGA_K"/>
                    </shape>
                    <scale>
                        <var varId="b_OMEGA_K"/>
                    </scale>
                </GammaDistribution>
            </RandomVariable>
            
            <RandomVariable symbId="TAU_V">
                <ct:VariabilityReference>
                    <ct:SymbRef symbIdRef="pri" blkIdRef="priorModel"/>
                </ct:VariabilityReference>
                <GammaDistribution xmlns="http://www.uncertml.org/3.0" definition="">
                    <shape>
                        <var varId="a_OMEGA_V"/>
                    </shape>
                    <scale>
                        <var varId="b_OMEGA_V"/>
                    </scale>
                </GammaDistribution>
            </RandomVariable>
            
            <RandomVariable symbId="TAU_T">
                <ct:VariabilityReference>
                    <ct:SymbRef symbIdRef="pri" blkIdRef="priorModel"/>
                </ct:VariabilityReference>
                <GammaDistribution xmlns="http://www.uncertml.org/3.0" definition="">
                    <shape>
                        <var varId="a_OMEGA_T"/>
                    </shape>
                    <scale>
                        <var varId="b_OMEGA_T"/>
                    </scale>
                </GammaDistribution>
            </RandomVariable>
            
            <IndividualParameter symbId="OMEGA_K">
                <ct:Assign>
                    <Equation xmlns="http://www.pharmml.org/2013/03/Maths">
                        <Binop op="divide">
                            <ct:Real>1</ct:Real>
                            <ct:SymbRef symbIdRef="TAU_K"/>
                        </Binop>
                    </Equation>
                </ct:Assign>
            </IndividualParameter>
            
            <IndividualParameter symbId="OMEGA_V">
                <ct:Assign>
                    <Equation xmlns="http://www.pharmml.org/2013/03/Maths">
                        <Binop op="divide">
                            <ct:Real>1</ct:Real>
                            <ct:SymbRef symbIdRef="TAU_V"/>
                        </Binop>
                    </Equation>
                </ct:Assign>
            </IndividualParameter>
            
            <IndividualParameter symbId="OMEGA_T">
                <ct:Assign>
                    <Equation xmlns="http://www.pharmml.org/2013/03/Maths">
                        <Binop op="divide">
                            <ct:Real>1</ct:Real>
                            <ct:SymbRef symbIdRef="TAU_T"/>
                        </Binop>
                    </Equation>
                </ct:Assign>
            </IndividualParameter>
            
            <!-- prior variables and transformations on "SIGMA" -->
            <RandomVariable symbId="POP_T">
                <ct:VariabilityReference>
                    <ct:SymbRef symbIdRef="pri" blkIdRef="priorModel"/>
                </ct:VariabilityReference>
                <GammaDistribution xmlns="http://www.uncertml.org/3.0" definition="">
                    <shape>
                        <var varId="a_POP_T"/>
                    </shape>
                    <scale>
                        <var varId="b_POP_T"/>
                    </scale>
                </GammaDistribution>
            </RandomVariable>
            
            
            <!-- IIV -->
            <RandomVariable symbId="eta_K">
                <ct:VariabilityReference>
                    <ct:SymbRef symbIdRef="indiv" blkIdRef="model"/>
                </ct:VariabilityReference>
                <NormalDistribution xmlns="http://www.uncertml.org/3.0" definition="">
                    <mean>
                        <rVal>0</rVal>
                    </mean>
                    <variance>
                        <var varId="OMEGA_K"/>
                    </variance>
                </NormalDistribution>
            </RandomVariable>
            
            <RandomVariable symbId="eta_V">
                <ct:VariabilityReference>
                    <ct:SymbRef symbIdRef="indiv" blkIdRef="model"/>
                </ct:VariabilityReference>
                <NormalDistribution xmlns="http://www.uncertml.org/3.0" definition="">
                    <mean>
                        <rVal>0</rVal>
                    </mean>
                    <variance>
                        <var varId="OMEGA_V"/>
                    </variance>
                </NormalDistribution>
            </RandomVariable>
            
            <RandomVariable symbId="eta_T">
                <ct:VariabilityReference>
                    <ct:SymbRef symbIdRef="indiv" blkIdRef="model"/>
                </ct:VariabilityReference>
                <NormalDistribution xmlns="http://www.uncertml.org/3.0" definition="">
                    <mean>
                        <rVal>0</rVal>
                    </mean>
                    <variance>
                        <var varId="OMEGA_T"/>
                    </variance>
                </NormalDistribution>
            </RandomVariable>
            
            <RandomVariable symbId="eps">
                <ct:VariabilityReference>
                    <ct:SymbRef symbIdRef="residual" blkIdRef="resErrorModel"/>
                </ct:VariabilityReference>
                <NormalDistribution xmlns="http://www.uncertml.org/3.0" definition="">
                    <mean>
                        <rVal>0</rVal>
                    </mean>
                    <variance>
                        <prVal>1</prVal>
                    </variance>
                </NormalDistribution>
            </RandomVariable>
            
            <IndividualParameter symbId="K">
                <GaussianModel>
                    <Transformation>log</Transformation>
                    <LinearCovariate>
                        <PopulationParameter>
                            <ct:Assign>
                                <ct:SymbRef symbIdRef="POP_K"/>
                            </ct:Assign>
                        </PopulationParameter>
                    </LinearCovariate>
                    <RandomEffects>
                        <ct:SymbRef symbIdRef="eta_K"/>
                    </RandomEffects>
                </GaussianModel>
            </IndividualParameter>
            
            <IndividualParameter symbId="V">
                <GaussianModel>
                    <Transformation>log</Transformation>
                    <LinearCovariate>
                        <PopulationParameter>
                            <ct:Assign>
                                <ct:SymbRef symbIdRef="POP_V"/>
                            </ct:Assign>
                        </PopulationParameter>
                    </LinearCovariate>
                    <RandomEffects>
                        <ct:SymbRef symbIdRef="eta_V"/>
                    </RandomEffects>
                </GaussianModel>
            </IndividualParameter>
            
            <IndividualParameter symbId="TAU">
                <GaussianModel>
                    <Transformation>log</Transformation>
                    <LinearCovariate>
                        <PopulationParameter>
                            <ct:Assign>
                                <ct:SymbRef symbIdRef="POP_T"/>
                            </ct:Assign>
                        </PopulationParameter>
                    </LinearCovariate>
                    <RandomEffects>
                        <ct:SymbRef symbIdRef="eta_T"/>
                    </RandomEffects>
                </GaussianModel>
            </IndividualParameter>
            
            <!-- SD_ADD can also be defined as SimpleParameter -->
            <IndividualParameter symbId="SD_ADD">
                <ct:Assign>
                    <Equation xmlns="http://www.pharmml.org/2013/03/Maths">
                        <Uniop op="sqrt">
                            <Binop op="divide">
                                <ct:Real>1</ct:Real>
                                <ct:SymbRef symbIdRef="TAU"/>
                            </Binop>
                        </Uniop>
                    </Equation>
                </ct:Assign>
            </IndividualParameter>

        </ParameterModel>
        
        <StructuralModel blkId="sm1">
            
            <ct:Variable symbId="C" symbolType="real">
                <ct:Assign>
                    <Equation xmlns="http://www.pharmml.org/2013/03/Maths">
                        <Binop op="times">
                            <Binop op="divide">
                                <ct:SymbRef symbIdRef="DOSE" blkIdRef="cm1"/>
                                <ct:SymbRef symbIdRef="V" blkIdRef="pm1"/>
                            </Binop>
                            <Uniop op="exp">
                                <Uniop op="minus">
                                    <Binop op="times">
                                        <ct:SymbRef symbIdRef="K" blkIdRef="pm1"/>
                                        <ct:SymbRef symbIdRef="T"/>
                                    </Binop>
                                </Uniop>
                            </Uniop>
                        </Binop>
                    </Equation>
                </ct:Assign>
            </ct:Variable>
            
        </StructuralModel>
        
        <ObservationModel blkId="om1">
            <ContinuousData>                
                <Standard symbId="Y">
                    <Output>
                        <ct:SymbRef symbIdRef="C" blkIdRef="sm1"/>
                    </Output>
                    <ErrorModel>
                        <ct:Assign>
                            <ct:SymbRef symbIdRef="SD_ADD" blkIdRef="pm1"/>
                        </ct:Assign>
                    </ErrorModel>
                    <ResidualError>
                        <ct:SymbRef symbIdRef="eps" blkIdRef="pm1"/>
                    </ResidualError>
                </Standard>
            </ContinuousData>
        </ObservationModel>
    </ModelDefinition>
    
    
    <ModellingSteps xmlns="http://www.pharmml.org/2013/03/ModellingSteps">
        
        <!-- THE FOLLOWING PROPOSED TAG ("RdataSet") IS TO USE PRIOR DATA FROM EXTERNAL FILE. IF IT IS DEFINED "INLINE" IN MDL, DATA SHOULD BE ASSIGNED AS SIMPLEPARAMETERS (?) -->
        <!--
        <RdataSet oid="Roid">
            <ElementMapping>
                <ElementRef xmlns="http://www.pharmml.org/2013/08/Dataset" elementIdRef="data_POP_K_V"/>
                <ct:SymbRef symbIdRef="data_POP_K_V" blkIdRef="pm1"/>
            </ElementMapping>
            
            <DataSet xmlns="http://www.pharmml.org/2013/08/Dataset">
                <Definition>
                    <Element elementId="data_POP_K" elementType="dataMatrix" valueType="real" elementNum="1"/>
                </Definition>
                <ImportData oid="id1">
                    <path>prior_bo1.txt</path>
                    <format>TXT???</format>
                    <delimiter>NOT USEFUL FOR R FORMAT DATA ??? </delimiter>
                </ImportData>
            </DataSet>
        </RdataSet>
         -->
        
        <NONMEMdataSet oid="NMoid">
            <ColumnMapping>
                <ColumnRef xmlns="http://www.pharmml.org/2013/08/Dataset" columnIdRef="TIME"/>
                <ct:SymbRef symbIdRef="T"/>
            </ColumnMapping>
            <ColumnMapping>
                <ColumnRef xmlns="http://www.pharmml.org/2013/08/Dataset" columnIdRef="DV"/>
                <ct:SymbRef symbIdRef="Y" blkIdRef="om1"/>
            </ColumnMapping>
            <ColumnMapping>
                <ColumnRef xmlns="http://www.pharmml.org/2013/08/Dataset" columnIdRef="DOSE"/>
                <ct:SymbRef symbIdRef="DOSE" blkIdRef="cm1"/>
            </ColumnMapping>
            
            <DataSet xmlns="http://www.pharmml.org/2013/08/Dataset">
                <Definition>
                    <Column columnId="ID" columnType="id" valueType="id" columnNum="1"/>
                    <Column columnId="TIME" columnType="idv" valueType="real" columnNum="2"/>
                    <Column columnId="DV" columnType="dv" valueType="real" columnNum="3"/>
                    <Column columnId="DOSE" columnType="covariate" valueType="real" columnNum="4"/>
                </Definition>
                <ImportData oid="id1">
                    <path>bo1.csv</path>
                    <format>CSV</format>
                    <delimiter>COMMA</delimiter>
                </ImportData>
            </DataSet>
        </NONMEMdataSet>
        
        
        <EstimationStep oid="est1">
            
            <NONMEMdataSetReference>
                <ct:OidRef oidRef="NMoid"/>
            </NONMEMdataSetReference>
            
            <!-- THE FOLLOWING PROPOSED TAG IS TO USE PRIOR DATA FROM EXTERNAL FILE. IF IT IS DEFINED "INLINE" IN MDL, THIS IS NOT NECESSARY -->
            <!-- 
            <RdataSetReference>
                <ct:OidRef oidRef="Roid"/>
            </RdataSetReference>
             -->
                        
            <ParametersToEstimate>
                <ParameterEstimation>
                    <ct:SymbRef symbIdRef="lPOP_P" blkIdRef="pm1"/>
                </ParameterEstimation>
                <ParameterEstimation>
                    <ct:SymbRef symbIdRef="OMEGA_K" blkIdRef="pm1"/>
                </ParameterEstimation>
                <ParameterEstimation>
                    <ct:SymbRef symbIdRef="OMEGA_V" blkIdRef="pm1"/>
                </ParameterEstimation>
                <ParameterEstimation>   
                    <ct:SymbRef symbIdRef="POP_T" blkIdRef="pm1"/>
                </ParameterEstimation>
                <ParameterEstimation>
                    <ct:SymbRef symbIdRef="OMEGA_T" blkIdRef="pm1"/>
                </ParameterEstimation>
            </ParametersToEstimate>
            
            <Operation order="1" opType="estPop"/>
        </EstimationStep>
        
        <StepDependencies>
            <Step>
                <ct:OidRef oidRef="est1"/>
            </Step>
        </StepDependencies>
        
    </ModellingSteps>
    
</PharmML>
