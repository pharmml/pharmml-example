                <ct:Assign><ct:Real>0.27</ct:Real></ct:Assign>
            </Parameter>
<!--            sc_F_eport   = 0.25;-->
            <Parameter symbId="sc_F_eport">
                <ct:Assign><ct:Real>0.25</ct:Real></ct:Assign>
