        <!-- STRUCTURAL MODEL - ODEs -->
        <StructuralModel blkId="sm1">
            
            <!-- dC/dt -->
            <ct:DerivativeVariable symbolType="real" symbId="C">
                <ct:Assign>
                    <math:Binop op="times">
                        <math:Uniop op="minus">
                            <ct:SymbRef blkIdRef="pm1" symbIdRef="KDE"/>
                        </math:Uniop>
                        <ct:SymbRef symbIdRef="C"/>
                    </math:Binop>
                </ct:Assign>
                <ct:IndependentVariable>
                    <ct:SymbRef symbIdRef="time"/>
                </ct:IndependentVariable>
                <ct:InitialCondition>
                    <ct:InitialValue>
                        <ct:Assign>
                            <ct:SymbRef blkIdRef="pm1" symbIdRef="C0"/>
                        </ct:Assign>
                    </ct:InitialValue>
                </ct:InitialCondition>
            </ct:DerivativeVariable>
            
            <!-- dPT/dt -->
            <ct:DerivativeVariable symbolType="real" symbId="PT">
                <ct:Assign>
                        <math:Binop op="plus">
                            <math:Binop op="times">
                                <ct:SymbRef blkIdRef="pm1" symbIdRef="LAMBDAP"/>
                                <math:Binop op="times">
                                    <ct:SymbRef symbIdRef="PT"/>
                                    <math:Binop op="minus">
                                        <ct:Real>1</ct:Real>                        <math:Binop op="divide">
                                            <ct:SymbRef symbIdRef="PSTAR"/>
                                            <ct:SymbRef blkIdRef="pm1" symbIdRef="K"/>
                                        </math:Binop>
                                    </math:Binop>
                                </math:Binop>
                            </math:Binop>
                            <math:Binop op="minus">
                                <math:Binop op="times">
                                    <ct:SymbRef blkIdRef="pm1" symbIdRef="KQPP"/>
                                    <ct:SymbRef symbIdRef="QP"/>
                                </math:Binop>
                                <math:Binop op="plus">
                                    <math:Binop op="times">
                                        <ct:SymbRef blkIdRef="pm1" symbIdRef="KPQ"/>
                                        <ct:SymbRef symbIdRef="PT"/>
                                    </math:Binop>
                                    <math:Binop op="times">
                                        <ct:SymbRef blkIdRef="pm1" symbIdRef="GAMMA"/>
                                        <math:Binop op="times">
                                            <ct:SymbRef symbIdRef="C"/>
                                            <math:Binop op="times">
                                                <ct:SymbRef blkIdRef="pm1" symbIdRef="KDE"/>
                                                <ct:SymbRef symbIdRef="PT"/>
                                            </math:Binop>
                                        </math:Binop>
                                    </math:Binop>
                                </math:Binop>
                            </math:Binop>
                        </math:Binop>
                </ct:Assign>
                <ct:IndependentVariable>
                    <ct:SymbRef symbIdRef="time"/>
                </ct:IndependentVariable>
                <ct:InitialCondition>
                    <ct:InitialValue>
                        <ct:Assign>
                            <ct:SymbRef blkIdRef="pm1" symbIdRef="P0"/>
                        </ct:Assign>
                    </ct:InitialValue>
                </ct:InitialCondition>
            </ct:DerivativeVariable>
            
            <!-- dQ/dt --> 
            <ct:DerivativeVariable symbolType="real" symbId="Q">
                <ct:Assign>
                    <math:Binop op="minus">
                        <math:Binop op="times">
                            <ct:SymbRef blkIdRef="pm1" symbIdRef="KPQ"/>
                            <ct:SymbRef symbIdRef="PT"/>
                        </math:Binop>
                        <math:Binop op="times">
                            <ct:SymbRef  blkIdRef="pm1" symbIdRef="GAMMA"/>
                            <math:Binop op="times">
                                <ct:SymbRef symbIdRef="C"/>
                                <math:Binop op="times">
                                    <ct:SymbRef blkIdRef="pm1" symbIdRef="KDE"/>
                                    <ct:SymbRef symbIdRef="Q"/>
                                </math:Binop>
                            </math:Binop>
                        </math:Binop>
                    </math:Binop>
                </ct:Assign>
                <ct:IndependentVariable>
                    <ct:SymbRef symbIdRef="time"/>
                </ct:IndependentVariable>
                <ct:InitialCondition>
                    <ct:InitialValue>
                        <ct:Assign>
                            <ct:SymbRef blkIdRef="pm1" symbIdRef="Q0"/>
                        </ct:Assign>
                    </ct:InitialValue>
                </ct:InitialCondition>
            </ct:DerivativeVariable>
            
            <!-- dQP/dt -->
            <ct:DerivativeVariable symbolType="real" symbId="QP">
                <ct:Assign>
                    <math:Binop op="minus">
                        <math:Binop op="minus">
                            <math:Binop op="times">
                                <ct:SymbRef blkIdRef="pm1" symbIdRef="GAMMA"/>
                                <math:Binop op="times">
                                    <ct:SymbRef symbIdRef="C"/>
                                    <math:Binop op="times">
                                        <ct:SymbRef blkIdRef="pm1" symbIdRef="KDE"/>
                                        <ct:SymbRef symbIdRef="Q"/>
                                    </math:Binop>
                                </math:Binop>
                            </math:Binop>
                            <math:Binop op="times">
                                <ct:SymbRef blkIdRef="pm1" symbIdRef="KQPP"/>
                                <ct:SymbRef symbIdRef="QP"/>
                            </math:Binop>
                        </math:Binop>
                        <math:Binop op="times">
                            <ct:SymbRef blkIdRef="pm1" symbIdRef="DELTAQP"/>
                            <ct:SymbRef symbIdRef="QP"/>
                        </math:Binop>                        
                    </math:Binop>
                </ct:Assign>
                <ct:IndependentVariable>
                    <ct:SymbRef symbIdRef="time"/>
                </ct:IndependentVariable>
                <ct:InitialCondition>
                    <ct:InitialValue>
                        <ct:Assign>
                            <ct:SymbRef blkIdRef="pm1" symbIdRef="QP0"/>
                        </ct:Assign>
                    </ct:InitialValue>
                </ct:InitialCondition>
            </ct:DerivativeVariable>

            <!-- PSTAR -->
            <ct:Variable symbolType="real" symbId="PSTAR" >
                <ct:Assign>
                    <math:Binop op="plus">
                        <ct:SymbRef symbIdRef="PT"/>
                        <math:Binop op="plus">
                            <ct:SymbRef symbIdRef="Q"/>
                            <ct:SymbRef symbIdRef="QP"/>
                        </math:Binop>
                    </math:Binop>
                </ct:Assign>
            </ct:Variable>
        </StructuralModel>


<!--        <ObservationModel blkId="om1">
            <ContinuousData>
