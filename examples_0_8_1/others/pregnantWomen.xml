            </Parameter>
<!--            sc_F_stomach = 0.52;-->
            <Parameter symbId="sc_F_stomach">
                <ct:Assign><ct:Real>0.52</ct:Real></ct:Assign>
            </Parameter>
<!--            sc_F_thyroid = 4.72;-->
            <Parameter symbId="sc_F_thyroid">
