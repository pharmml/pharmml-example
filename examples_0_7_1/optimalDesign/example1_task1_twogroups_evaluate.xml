<?xml version="1.0" encoding="UTF-8"?>
<PharmML xmlns="http://www.pharmml.org/pharmml/0.7/PharmML"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.pharmml.org/pharmml/0.7/PharmML http://www.pharmml.org/pharmml/0.7/PharmML"
    xmlns:ct="http://www.pharmml.org/pharmml/0.7/CommonTypes"
    xmlns:ds="http://www.pharmml.org/pharmml/0.7/Dataset"
    implementedBy="MJS" writtenVersion="0.7.1"
    metadataFile="task1_twogroups_evaluate.rdf" id="i1">
    
    
    <!-- MODEL NAME -->
    <ct:Name>task1_twogroups_evaluate - design only</ct:Name>
    
    <ct:Description>based on task1_twogroups_evaluate.mld - design only with dummy model definition</ct:Description>
    
    <!-- INDEPENDENT VARIABLE -->
    <IndependentVariable symbId="time"/>

    <!-- BLOCK I: MODEL DEFINITION -->
    <ModelDefinition  xmlns="http://www.pharmml.org/pharmml/0.7/ModelDefinition">
        
        <VariabilityModel blkId="vm1" type="parameterVariability">
            <Level symbId="subject"/>
        </VariabilityModel>
        
        <!-- PARAMETER MODEL -->
        <ParameterModel blkId="pm1">
            <PopulationParameter symbId="a"/>
        </ParameterModel>
        
        <StructuralModel blkId="sm1">
            <ct:Variable symbolType="real" symbId="A"/>
        </StructuralModel>
        
        <ObservationModel blkId="om1">
            <ContinuousData>
                <PopulationParameter symbId="a"/>
                <Standard symbId="Y">
                    <Output>
                        <ct:SymbRef blkIdRef="sm1" symbIdRef="A"/>
                    </Output>
                    <ErrorModel>
                        <ct:Assign>
                            <ct:Real>1</ct:Real>
                        </ct:Assign>
                    </ErrorModel>
                    <ResidualError>
                        <ct:SymbRef symbIdRef="blub"/>
                    </ResidualError>
                </Standard>
            </ContinuousData>
        </ObservationModel>

    </ModelDefinition>
    
    <!-- BLOCK II: TRIAL DEFINITION -->
    <TrialDesign xmlns="http://www.pharmml.org/pharmml/0.7/TrialDesign">
        
        <Interventions>
            <Administration oid="admin1">
                <Bolus>
                    <DoseAmount inputTarget="admType">
                        <TargetMapping blkIdRef="sm1">
                            <ds:Map modelSymbol="CENTRAL"/>
                        </TargetMapping>
                        <ct:Assign>
                            <ct:Int>100</ct:Int>              
                        </ct:Assign>
                    </DoseAmount>
                    <DosingTimes>
                        <ct:Assign>
                            <ct:Real>0</ct:Real>
                        </ct:Assign>
                    </DosingTimes>
                </Bolus>
            </Administration>
            
            <Administration oid="admin2">
                <Bolus>
                    <DoseAmount inputTarget="admType">
                        <TargetMapping blkIdRef="sm1">
                            <ds:Map modelSymbol="CENTRAL"/>
                        </TargetMapping>
                        <ct:Assign>
                            <ct:Int>200</ct:Int>              
                        </ct:Assign>
                    </DoseAmount>
                    <DosingTimes>
                        <ct:Assign>
                            <ct:Real>0</ct:Real>
                        </ct:Assign>
                    </DosingTimes>
                </Bolus>
            </Administration>
        </Interventions>
        
        <Observations>
            <Observation oid="window1">
                <ObservationTimes>
                    <ct:Assign>
                        <ct:Vector>
                            <ct:VectorElements>
                                <ct:Real>0.5</ct:Real>
                                <ct:Real>2</ct:Real>
                                <ct:Real>30</ct:Real>
                            </ct:VectorElements>
                        </ct:Vector>
                    </ct:Assign>
                </ObservationTimes>
                <Continuous>
                    <ct:SymbRef blkIdRef="om1" symbIdRef="Y"/> <!-- CHECK THAT -->
                </Continuous>
            </Observation>
            
            <Observation oid="window2">
                <ObservationTimes>
                    <ct:Assign>
                        <ct:Vector>
                            <ct:VectorElements>
                                <ct:Real>0.5</ct:Real>
                                <ct:Real>2</ct:Real>
                                <ct:Real>16</ct:Real>
                            </ct:VectorElements>
                        </ct:Vector>
                    </ct:Assign>
                </ObservationTimes>
                <Continuous>
                    <ct:SymbRef blkIdRef="om1" symbIdRef="Y"/> <!-- CHECK THAT -->
                </Continuous>
            </Observation>
        </Observations>
        
        <Arms>
            <ArmSize>
                <ct:Assign>
                    <ct:Int>20</ct:Int>
                </ct:Assign>
            </ArmSize>
            <Arm oid="arm1">
                <ArmSize>
                    <ct:Assign>
                        <ct:Int>10</ct:Int>
                    </ct:Assign>
                </ArmSize>
                <InterventionSequence>
                    <InterventionList>
                        <InterventionRef oidRef="admin1"/>
                    </InterventionList>
                    <Start>
                        <ct:Assign>
                            <ct:Real>0</ct:Real>
                        </ct:Assign>
                    </Start>
                </InterventionSequence>
                <ObservationSequence>
                    <ObservationList>
                        <ObservationRef oidRef="window1"/>
                    </ObservationList>
                    <Start>
                        <ct:Assign>
                            <ct:Real>0</ct:Real>
                        </ct:Assign>
                    </Start>
                </ObservationSequence>
            </Arm>
            
            <Arm oid="arm2">
                <InterventionSequence>
                    <InterventionList>
                        <InterventionRef oidRef="admin2"/>
                    </InterventionList>
                    <Start>
                        <ct:Assign>
                            <ct:Real>0</ct:Real>
                        </ct:Assign>
                    </Start>
                </InterventionSequence>
                <ObservationSequence>
                    <ObservationList>
                        <ObservationRef oidRef="window2"/>
                    </ObservationList>
                    <Start>
                        <ct:Assign>
                            <ct:Real>0</ct:Real>
                        </ct:Assign>
                    </Start>
                </ObservationSequence>
            </Arm>
        </Arms>
    </TrialDesign>
    
    <!-- BLOCK III: MODELLING STEPS -->
    <ModellingSteps xmlns="http://www.pharmml.org/pharmml/0.7/ModellingSteps">
        
        <!-- ESTIMATION STEP -->
        <EstimationStep oid="estTask1">
            
            <ParametersToEstimate>

                <!-- a -->
                <ParameterEstimation>
                    <ct:SymbRef blkIdRef="pm1" symbIdRef="a"/>
                    <InitialEstimate fixed="false">
                        <ct:Real>10</ct:Real>
                    </InitialEstimate>
                </ParameterEstimation>
                
            </ParametersToEstimate>
            <Operation order="1" opType="estPop"/>
            <Operation order="2" opType="estIndiv"/>
        </EstimationStep>

        <!-- STEP DEPENDENCIES -->
        <StepDependencies>
            <Step>
                <ct:OidRef oidRef="estTask1"/>
            </Step>
        </StepDependencies>
    </ModellingSteps>
</PharmML>
